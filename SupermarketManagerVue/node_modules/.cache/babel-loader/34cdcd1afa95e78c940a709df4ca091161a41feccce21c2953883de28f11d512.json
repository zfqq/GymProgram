{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { clearCookie, loginEmp } from \"@/assets/js/auth\";\nimport { ajaxGet, ajaxPost, popup } from \"@/assets/js/common\";\nimport Cookies from \"js-cookie\";\nexport default {\n  data() {\n    return {\n      BaseApi: this.$store.state.BaseApi,\n      personalFulVisable: false,\n      logoutVisable: false,\n      loginName: \"\",\n      logoutform: {},\n      circleUrl: loginEmp().headImg,\n      menu_catalogs: [],\n      rules: {\n        content: [{\n          required: true,\n          message: \"内容不能为空\",\n          trigger: \"blur\"\n        }]\n      }\n    };\n  },\n  methods: {\n    init() {\n      this.loginName = loginEmp().nickName;\n      ajaxGet(\"/empMenu\", {}).then(res => {\n        res = res.data;\n        if (res.code == 200) {\n          this.menu_catalogs = res.data;\n        }\n      });\n    },\n    /*个人资料*/\n    informationBtn() {\n      this.$router.push(\"/personal/information\");\n      popup(\"请完善个人的资料\");\n    },\n    /*账户退出*/\n    empExit() {\n      this.$confirm(\"确定要退出系统?\", \"警示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(() => {\n        ajaxGet(\"/exit\", this.pwdForm).then(res => {\n          res = res.data;\n          if (res.code == 200) {\n            popup(\"成功退出系统...\");\n            clearCookie(\"employee\");\n            clearCookie(\"token\");\n            this.$router.push(\"/\");\n          } else {\n            popup(res.msg, \"error\");\n          }\n        });\n      }).catch(() => {\n        this.$message({\n          type: \"info\",\n          message: \"已取消操作\"\n        });\n      });\n    },\n    /*注销账户*/\n    logoutCel(formName) {\n      this.$refs[formName].resetFields();\n      this.logoutform = {};\n      this.logoutVisable = false;\n    },\n    logoutSubmit(formName) {\n      this.$refs[formName].validate(valid => {\n        if (valid) {\n          ajaxPost(\"/logout\", {\n            content: this.logoutform.content\n          }).then(res => {\n            res = res.data;\n            if (res.code == 200) {\n              popup(\"注销成功\");\n              clearCookie(\"employee\");\n              clearCookie(\"token\");\n              this.logoutVisable = false;\n              this.$router.push(\"/\");\n            } else {\n              popup(res.msg, \"error\");\n            }\n          });\n        }\n      });\n    }\n  },\n  mounted() {\n    this.init();\n  }\n};","map":{"version":3,"names":["clearCookie","loginEmp","ajaxGet","ajaxPost","popup","Cookies","data","BaseApi","$store","state","personalFulVisable","logoutVisable","loginName","logoutform","circleUrl","headImg","menu_catalogs","rules","content","required","message","trigger","methods","init","nickName","then","res","code","informationBtn","$router","push","empExit","$confirm","confirmButtonText","cancelButtonText","type","pwdForm","msg","catch","$message","logoutCel","formName","$refs","resetFields","logoutSubmit","validate","valid","mounted"],"sources":["src/views/index.vue"],"sourcesContent":["<template>\n    <el-container>\n        <el-header>\n            <div\n                class=\"avatar\"\n                @click=\"personalFulVisable = !personalFulVisable\"\n            >\n                <div class=\"demo-basic--circle\">\n                    <div class=\"block\">\n                        <el-avatar :size=\"50\" :src=\"BaseApi + circleUrl\"></el-avatar>\n                        {{ loginName }}\n                    </div>\n                </div>\n            </div>\n            <i style=\"color: white;font-size: 32px;\" class=\"iconfont icon-r-building\"> 超市管理系统</i>\n        </el-header>\n        <!--个人设置菜单-->\n        <div\n            class=\"personalFul\"\n            v-show=\"personalFulVisable\"\n            @mouseout=\"personalFulVisable = !personalFulVisable\"\n        >\n            <ul>\n                <li @click=\"informationBtn\">个人资料完善</li>\n                <li @click=\"empExit\">退出</li>\n                <li @click=\"logoutVisable = true\">注销</li>\n            </ul>\n        </div>\n        <!--注销账户-->\n        <el-dialog title=\"注销账户\" :visible.sync=\"logoutVisable\" width=\"70%\">\n            <el-form\n                :model=\"logoutform\"\n                :rules=\"rules\"\n                ref=\"logoutform\"\n                label-width=\"100px\"\n                class=\"demo-ruleForm\"\n            >\n                <el-form-item label=\"内容\" prop=\"content\">\n                    <el-input\n                        v-model=\"logoutform.content\"\n                        placeholder=\"请填写“本人确定注销”\"\n                    ></el-input>\n                </el-form-item>\n                <el-form-item>\n                    <el-button\n                        type=\"primary\"\n                        @click=\"logoutSubmit('logoutform')\"\n                        >确 定</el-button\n                    >\n                    <el-button @click=\"logoutCel('logoutform')\"\n                        >取 消</el-button\n                    >\n                </el-form-item>\n            </el-form>\n        </el-dialog>\n\n        <el-container>\n            <el-aside width=\"200px\" style=\"overflow-y: hidden;\">\n                <el-menu\n                    background-color=\"#6495ED\"\n                    text-color=\"white\"\n                    :router=\"true\"\n                    :unique-opened=\"true\"\n                    active-text-color=\"black\"\n                >\n                    <el-submenu\n                        v-for=\"item in menu_catalogs\"\n                        :key=\"item.id\"\n                        :index=\"item.id + ''\"\n                    >\n                        <template slot=\"title\">\n                            <i :class=\"item.icon\" style=\"font-size: 26px;color: white;\">\n                                <b style=\"font-size: 18px\"> {{ item.label }}</b>\n                            </i>\n                        </template>\n                        <el-menu-item-group>\n                            <el-menu-item\n                                v-for=\"c in item.children\"\n                                :key=\"c.id\"\n                                :index=\"c.purl\"\n                            >\n                                <i :class=\"c.icon\" style=\"font-size: 24px;\">\n                                </i>\n                                    <b style=\"font-size: 16px\">\n                                        {{ c.label }}</b\n                                    >\n                            </el-menu-item>\n                        </el-menu-item-group>\n                    </el-submenu>\n                </el-menu>\n            </el-aside>\n            <el-main>\n                <router-view />\n            </el-main>\n        </el-container>\n    </el-container>\n</template>\n<script>\nimport { clearCookie, loginEmp } from \"@/assets/js/auth\";\nimport { ajaxGet, ajaxPost, popup } from \"@/assets/js/common\";\nimport Cookies from \"js-cookie\";\n\nexport default {\n    data() {\n        return {\n            BaseApi: this.$store.state.BaseApi,\n            personalFulVisable: false,\n            logoutVisable: false,\n            loginName: \"\",\n            logoutform: {},\n            circleUrl: loginEmp().headImg,\n            menu_catalogs: [],\n            rules: {\n                content: [\n                    {\n                        required: true,\n                        message: \"内容不能为空\",\n                        trigger: \"blur\",\n                    },\n                ],\n            },\n        };\n    },\n    methods: {\n        init() {\n            this.loginName = loginEmp().nickName;\n            ajaxGet(\"/empMenu\", {}).then((res) => {\n                res = res.data;\n                if (res.code == 200) {\n                    this.menu_catalogs = res.data;\n                }\n            });\n        },\n        /*个人资料*/\n        informationBtn() {\n            this.$router.push(\"/personal/information\");\n            popup(\"请完善个人的资料\");\n        },\n        /*账户退出*/\n        empExit() {\n            this.$confirm(\"确定要退出系统?\", \"警示\", {\n                confirmButtonText: \"确定\",\n                cancelButtonText: \"取消\",\n                type: \"warning\",\n            })\n                .then(() => {\n                    ajaxGet(\"/exit\", this.pwdForm).then((res) => {\n                        res = res.data;\n                        if (res.code == 200) {\n                            popup(\"成功退出系统...\");\n                            clearCookie(\"employee\");\n                            clearCookie(\"token\");\n                            this.$router.push(\"/\");\n                        } else {\n                            popup(res.msg, \"error\");\n                        }\n                    });\n                })\n                .catch(() => {\n                    this.$message({\n                        type: \"info\",\n                        message: \"已取消操作\",\n                    });\n                });\n        },\n        /*注销账户*/\n        logoutCel(formName) {\n            this.$refs[formName].resetFields();\n            this.logoutform = {};\n            this.logoutVisable = false;\n        },\n        logoutSubmit(formName) {\n            this.$refs[formName].validate((valid) => {\n                if (valid) {\n                    ajaxPost(\"/logout\", {\n                        content: this.logoutform.content,\n                    }).then((res) => {\n                        res = res.data;\n                        if (res.code == 200) {\n                            popup(\"注销成功\");\n                            clearCookie(\"employee\");\n                            clearCookie(\"token\");\n                            this.logoutVisable = false;\n                            this.$router.push(\"/\");\n                        } else {\n                            popup(res.msg, \"error\");\n                        }\n                    });\n                }\n            });\n        },\n    },\n    mounted() {\n        this.init();\n    },\n};\n</script>\n<style>\n\n.avatar {\n    display: inline-block;\n    width: 200px;\n    float: right;\n}\n\n.personalFul {\n    background-color: #ffffff;\n    position: absolute;\n    z-index: 999999;\n    top: 58px;\n    right: 20px;\n    width: 151px;\n}\n\n/* .personalFul ul > li {\n    margin: 1px auto;\n    padding: 0px;\n    height: 20px;\n    color: #ffffff;\n    background-color: #6495ED;\n    list-style-type: none;\n} */\n\n.personalFul ul > li:hover {\n    background-color: #fc4316;\n}\n\n.el-menu-item i {\n    color: white;\n}\n</style>\n"],"mappings":";AAkGA,SAAAA,WAAA,EAAAC,QAAA;AACA,SAAAC,OAAA,EAAAC,QAAA,EAAAC,KAAA;AACA,OAAAC,OAAA;AAEA;EACAC,KAAA;IACA;MACAC,OAAA,OAAAC,MAAA,CAAAC,KAAA,CAAAF,OAAA;MACAG,kBAAA;MACAC,aAAA;MACAC,SAAA;MACAC,UAAA;MACAC,SAAA,EAAAb,QAAA,GAAAc,OAAA;MACAC,aAAA;MACAC,KAAA;QACAC,OAAA,GACA;UACAC,QAAA;UACAC,OAAA;UACAC,OAAA;QACA;MAEA;IACA;EACA;EACAC,OAAA;IACAC,KAAA;MACA,KAAAX,SAAA,GAAAX,QAAA,GAAAuB,QAAA;MACAtB,OAAA,iBAAAuB,IAAA,CAAAC,GAAA;QACAA,GAAA,GAAAA,GAAA,CAAApB,IAAA;QACA,IAAAoB,GAAA,CAAAC,IAAA;UACA,KAAAX,aAAA,GAAAU,GAAA,CAAApB,IAAA;QACA;MACA;IACA;IACA;IACAsB,eAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;MACA1B,KAAA;IACA;IACA;IACA2B,QAAA;MACA,KAAAC,QAAA;QACAC,iBAAA;QACAC,gBAAA;QACAC,IAAA;MACA,GACAV,IAAA;QACAvB,OAAA,eAAAkC,OAAA,EAAAX,IAAA,CAAAC,GAAA;UACAA,GAAA,GAAAA,GAAA,CAAApB,IAAA;UACA,IAAAoB,GAAA,CAAAC,IAAA;YACAvB,KAAA;YACAJ,WAAA;YACAA,WAAA;YACA,KAAA6B,OAAA,CAAAC,IAAA;UACA;YACA1B,KAAA,CAAAsB,GAAA,CAAAW,GAAA;UACA;QACA;MACA,GACAC,KAAA;QACA,KAAAC,QAAA;UACAJ,IAAA;UACAf,OAAA;QACA;MACA;IACA;IACA;IACAoB,UAAAC,QAAA;MACA,KAAAC,KAAA,CAAAD,QAAA,EAAAE,WAAA;MACA,KAAA9B,UAAA;MACA,KAAAF,aAAA;IACA;IACAiC,aAAAH,QAAA;MACA,KAAAC,KAAA,CAAAD,QAAA,EAAAI,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA3C,QAAA;YACAe,OAAA,OAAAL,UAAA,CAAAK;UACA,GAAAO,IAAA