ailStoreVisible = true;\n        } else {\n          popup(res.msg, \"error\");\n        }\n      });\n    },\n    detailStoreBtn(row) {\n      this.options_storeGoods = [];\n      this.totalStoreNum1 = \"\";\n      this.searchDetailForm.storeId = row.storeId;\n      this.init1();\n    },\n    handleSizeChange1(val) {\n      this.searchDetailForm.pageSize = val;\n      this.init1();\n      console.log(`每页 ${val} 条`);\n    },\n    handleCurrentChange1(val) {\n      this.searchDetailForm.currentPage = val;\n      this.init1();\n      console.log(`当前页: ${val}`);\n    },\n    submitDetailSearchForm() {\n      this.init1();\n    },\n    customColorMethod(percentage) {\n      if (percentage < 30) {\n        return \"#f56c6c\";\n      } else if (percentage < 70) {\n        return \"#e6a23c\";\n      } else {\n        return \"#67c23a\";\n      }\n    },\n    closeDetailStoreVisible() {\n      this.searchDetailForm = {\n        total: 0,\n        currentPage: 1,\n        pageSize: 10\n      };\n      this.detailStoreVisible = false;\n    }\n  },\n  mounted() {\n    this.init();\n  }\n};","map":{"version":3,"names":["popup","queryPageByQo","queryStoreGoodsByStoreId","data","tableData","tableData1","totalStoreNum","totalStoreNum1","searchForm","total","currentPage","pageSize","searchDetailForm","options_storeGoods","detailStoreVisible","methods","init","then","res","code","page","records","size","current","msg","handleSizeChange","val","console","log","handleCurrentChange","submitSearchForm","init1","optionsStoreGoods","vos","detailStoreBtn","row","storeId","handleSizeChange1","handleCurrentChange1","submitDetailSearchForm","customColorMethod","percentage","closeDetailStoreVisible","mounted"],"sources":["src/views/inventory_management/store/StorageSituation.vue"],"sourcesContent":["<template>\n    <div>\n        <el-breadcrumb separator=\"/\">\n            <el-breadcrumb-item>首页</el-breadcrumb-item>\n            <el-breadcrumb-item>库存管理 </el-breadcrumb-item>\n            <el-breadcrumb-item>库存统计</el-breadcrumb-item> </el-breadcrumb\n        ><br />\n\n        <el-row>\n            <el-col :span=\"8\">\n                <el-input placeholder=\"仓库名称\" v-model=\"searchForm.name\" />\n            </el-col>\n            <el-col :span=\"16\">\n                <i class=\"iconfont icon-r-add\" style=\"font-size: 28px\">\n                    <b>\n                        总存储量：<span style=\"color: #fc4316\"\n                            >{{ totalStoreNum ? totalStoreNum : 0 }}件</span\n                        >\n                    </b>\n                </i>\n            </el-col>\n        </el-row>\n        <br />\n        <el-row>\n            <el-col :span=\"24\" style=\"text-align: left\">\n                <el-button\n                    type=\"primary\"\n                    @click=\"submitSearchForm\"\n                    style=\"font-size: 18px\"\n                >\n                    <i class=\"iconfont icon-r-find\" style=\"font-size: 18px\">\n                    </i>\n                    搜索</el-button\n                >\n            </el-col>\n        </el-row>\n        <br />\n        <div class=\"table\">\n            <el-table :data=\"tableData\" style=\"width: 100%\" size=\"medium\">\n                <el-table-column type=\"index\" width=\"200\" label=\"序号\">\n                </el-table-column>\n                <el-table-column\n                    prop=\"storeName\"\n                    :show-overflow-tooltip=\"true\"\n                    label=\"仓库名\"\n                >\n                </el-table-column>\n                <el-table-column\n                    prop=\"residueNum\"\n                    :show-overflow-tooltip=\"true\"\n                    label=\"存货数量\"\n                >\n                </el-table-column>\n                <el-table-column label=\"操作\">\n                    <template v-slot=\"scope\">\n                        <el-button\n                            type=\"primary\"\n                            style=\"font-size: 18px\"\n                            @click=\"detailStoreBtn(scope.row)\"\n                        >\n                            <i\n                                class=\"iconfont icon-r-find\"\n                                style=\"font-size: 18px\"\n                            ></i>\n                            存储明细</el-button\n                        >\n                    </template>\n                </el-table-column>\n            </el-table>\n            <div style=\"margin: 10px 0 15px 0\">\n                <el-pagination\n                    @size-change=\"handleSizeChange\"\n                    @current-change=\"handleCurrentChange\"\n                    :current-page.sync=\"searchForm.currentPage\"\n                    :page-sizes=\"[5, 10, 20, 50]\"\n                    :page-size=\"searchForm.pageSize\"\n                    layout=\"total,sizes, prev, pager, next,jumper\"\n                    :total=\"searchForm.total\"\n                >\n                </el-pagination>\n            </div>\n        </div>\n        <el-dialog\n            title=\"仓库存储明细\"\n            :visible.sync=\"detailStoreVisible\"\n            width=\"95%\"\n            :fullscreen=\"false\"\n        >\n            <el-row>\n                <el-col :span=\"8\" style=\"text-align: right;\">\n                    <el-select\n                        v-model=\"searchDetailForm.id\"\n                        placeholder=\"请选择商品\"\n                        filterable\n                        @change=\"$forceUpdate()\"\n                        clearable\n                    >\n                        <el-option\n                            v-for=\"item in options_storeGoods\"\n                            :key=\"item.id\"\n                            :label=\"item.name\"\n                            :value=\"item.id\"\n                        >\n                        </el-option>\n                    </el-select>\n                </el-col>\n                <el-col :span=\"8\">\n                    <el-button type=\"success\" @click=\"submitDetailSearchForm\" style=\"font-size: 16px;\"\n                        >\n                        <i class=\"iconfont icon-r-find\"></i>\n                         搜索</el-button\n                    >\n                    <el-button type=\"warning\" @click=\"closeDetailStoreVisible\" style=\"font-size: 16px;\"\n                        >\n                        <i class=\"iconfont icon-r-left\"></i>\n                         关闭</el-button\n                    >\n                </el-col>\n                <el-col :span=\"8\">\n                    \n                    <span\n                        >总存储量：<span style=\"color: #fc4316\"\n                            >{{ totalStoreNum1 ? totalStoreNum1 : 0 }}件</span\n                        ></span\n                    >\n                </el-col>\n            </el-row>\n            <br>\n            <div class=\"table\">\n                <el-table :data=\"tableData1\" style=\"width: 100%\" size=\"medium\">\n                    <el-table-column type=\"index\" width=\"200\" label=\"序号\">\n                    </el-table-column>\n                    <el-table-column\n                        prop=\"goodsName\"\n                        :show-overflow-tooltip=\"true\"\n                        label=\"商品名\"\n                    >\n                    </el-table-column>\n                    <el-table-column\n                        prop=\"residueNum\"\n                        :show-overflow-tooltip=\"true\"\n                        label=\"商品数量\"\n                    >\n                    </el-table-column>\n                    <el-table-column\n                        prop=\"percentage\"\n                        :show-overflow-tooltip=\"true\"\n                        label=\"占总存储量\"\n                    >\n                        <template v-slot=\"scope\">\n                            <el-progress\n                                type=\"circle\"\n              