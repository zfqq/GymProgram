{"ast":null,"code":"import { popup } from \"@/assets/js/common\";\nimport { deactivate, listByQo, save, update } from \"@/api/personnel_management/department/deptApi\";\nexport default {\n  data() {\n    return {\n      tableData: [],\n      editForm: {},\n      editRow: {},\n      dialogVisible: false,\n      newVisable: false,\n      newForm: {},\n      searchForm: {},\n      options: [{\n        label: '正常',\n        value: '0'\n      }, {\n        label: '停用',\n        value: '-1'\n      }],\n      rules: {\n        name: [{\n          required: true,\n          message: '名称不能为空',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  methods: {\n    init() {\n      listByQo(this.searchForm).then(res => {\n        if (res.data.code == 200) {\n          this.tableData = res.data.data;\n        } else {\n          popup(res.data.msg, \"error\");\n        }\n      });\n    },\n    forbidden(id) {\n      deactivate(id).then(res => {\n        res = res.data;\n        if (res.code == 200) {\n          popup(\"操作成功\");\n        } else {\n          popup(res.msg, \"error\");\n        }\n        this.init();\n      });\n    },\n    /*弹出修改表单*/\n    edit(row) {\n      this.editForm = {\n        ...row\n      };\n      this.dialogVisible = true;\n    },\n    /*条件搜索*/\n    subSearchForm() {\n      this.init();\n    },\n    /*对接后端修改接口*/\n    submitEditForm() {\n      update(this.editForm).then(res => {\n        res = res.data;\n        if (res.code == 200) {\n          popup(\"操作成功\");\n          this.dialogVisible = false;\n          this.init();\n        } else {\n          popup(res.msg, \"error\");\n        }\n      });\n    },\n    /*修改重置按钮*/\n    canCelEdit() {\n      this.dialogVisible = false;\n      this.editForm = {};\n    },\n    /*创建新角色*/\n    submitNewForm(formName) {\n      this.$refs[formName].validate(valid => {\n        if (valid) {\n          console.log(this.newForm);\n          save(this.newForm).then(res => {\n            res = res.data;\n            if (res.code == 200) {\n              popup(\"操作成功\");\n              this.newVisable = false;\n              this.newForm = {};\n              this.init();\n            } else {\n              popup(res.msg, \"error\");\n            }\n          });\n        }\n      });\n    },\n    resetNewForm(formname) {\n      this.$refs[formname].resetFields();\n    }\n  },\n  created() {\n    this.init();\n  }\n};","map":{"version":3,"names":["popup","deactivate","listByQo","save","update","data","tableData","editForm","editRow","dialogVisible","newVisable","newForm","searchForm","options","label","value","rules","name","required","message","trigger","methods","init","then","res","code","msg","forbidden","id","edit","row","subSearchForm","submitEditForm","canCelEdit","submitNewForm","formName","$refs","validate","valid","console","log","resetNewForm","formname","resetFields","created"],"sources":["src/views/personnel_management/dept/List.vue"],"sourcesContent":["<template>\n    <div id=\"dept_list\">\n        <el-breadcrumb separator=\"/\">\n            <el-breadcrumb-item\n                >首页</el-breadcrumb-item\n            >\n            <el-breadcrumb-item\n                >人事管理\n            </el-breadcrumb-item>\n            <el-breadcrumb-item\n                >部门管理</el-breadcrumb-item\n            > </el-breadcrumb\n        ><br />\n\n        <el-row>\n            <el-col :span=\"7\">\n                <el-input placeholder=\"部门名称\" v-model=\"searchForm.name\"/>\n            </el-col>\n            <el-col :span=\"7\">\n                <el-select v-model=\"searchForm.state\" placeholder=\"请选择状态\" clearable>\n                    <el-option label=\"正常\" value=\"0\"></el-option>\n                    <el-option label=\"停用\" value=\"-1\"></el-option>\n                </el-select>\n            </el-col>\n        </el-row>\n        <br />\n        <el-row>\n            <el-col :span=\"24\" style=\"text-align: left\">\n                <el-button\n                    type=\"primary\"\n                    @click=\"subSearchForm\"\n                    style=\"font-size: 18px\"\n                >\n                    <i class=\"iconfont icon-r-find\" style=\"font-size: 18px\">\n                    </i>\n                    搜索</el-button\n                >\n                <el-button type=\"success\" @click=\"newVisable=true\"\n                    style=\"font-size: 18px\"\n                >\n                    <i class=\"iconfont icon-r-add\" style=\"font-size: 18px\">\n                    </i> 创建部门\n                </el-button>\n            </el-col>\n        </el-row>\n        <br />\n        \n        <el-table\n                :data=\"tableData\"\n                style=\"width: 100%\">\n            <el-table-column\n                    type=\"index\"\n                    label=\"序号\"\n                    width=\"180\">\n            </el-table-column>\n            <el-table-column\n                    prop=\"name\"\n                    label=\"名称\"\n                    width=\"180\">\n            </el-table-column>\n            <el-table-column\n                    prop=\"info\"\n                    label=\"描述\">\n            </el-table-column>\n            <el-table-column\n                    prop=\"state\"\n                    label=\"状态\">\n                <template v-slot=\"scope\">\n                    <el-tag v-if=\"scope.row.state=='0'\" type=\"success\">正常</el-tag>\n                    <el-tag v-else type=\"danger\">停用</el-tag>\n                </template>\n            </el-table-column>\n            <el-table-column\n                    label=\"操作\">\n                <template v-slot=\"scope\">\n                    <el-button size=\"mini\" type=\"warning\" @click=\"edit(scope.row)\" plain>修改</el-button>\n                    <el-button size=\"mini\" type=\"danger\" @click=\"forbidden(scope.row.id)\" plain>停用</el-button>\n                </template>\n            </el-table-column>\n        </el-table>\n        <!--修改描述/状态弹出框-->\n        <el-dialog\n                title=\"部门修改\"\n                :visible.sync=\"dialogVisible\"\n                width=\"50%\">\n            <el-form v-model=\"editForm\"  label-width=\"100px\" class=\"demo-ruleForm\">\n                <el-form-item label=\"名称：\">\n                    <el-input disabled v-model=\"editForm.name\"></el-input>\n                </el-form-item>\n                <el-form-item label=\"描述：\">\n                    <el-input type=\"text\" v-model=\"editForm.info\"></el-input>\n                </el-form-item>\n                <el-form-item label=\"状态：\">\n                    <el-select v-model=\"editForm.state\"\n                               clearable\n                               @change=\"$forceUpdate()\"\n                               placeholder=\"请选择状态\">\n                        <el-option\n                                v-for=\"item in options\"\n                                :key=\"item.value\"\n                                :label=\"item.label\"\n                                :value=\"item.value\">\n                        </el-option>\n                    </el-select>\n                </el-form-item>\n                <el-form-item>\n                    <el-button type=\"primary\" @click=\"submitEditForm\">提交</el-button>\n                    <el-button @click=\"canCelEdit\">关闭</el-button>\n                </el-form-item>\n            </el-form>\n        </el-dialog>\n        <!--创建弹出框-->\n        <el-dialog\n                title=\"创建部门\"\n                :visible.sync=\"newVisable\"\n                width=\"50%\">\n            <el-form :model=\"newForm\" :rules=\"rules\" ref=\"newForm\"  label-width=\"100px\" class=\"demo-ruleForm\">\n                <el-form-item label=\"名称：\" prop=\"name\">\n                    <el-input v-model=\"newForm.name\"></el-input>\n                </el-form-item>\n                <el-form-item label=\"描述：\">\n                    <el-input type=\"text\" v-model=\"newForm.info\"></el-input>\n                </el-form-item>\n                <el-form-item>\n                    <el-button type=\"primary\" @click=\"submitNewForm('newForm')\">提交</el-button>\n                    <el-button @click=\"resetNewForm('newForm')\">重置</el-button>\n                </el-form-item>\n            </el-form>\n        </el-dialog>\n    </div>\n</template>\n<script>\n    import { popup} from \"@/assets/js/common\";\n    import {deactivate, listByQo, save, update} from \"@/api/personnel_management/department/deptApi\";\n\n    export default {\n        data() {\n            return {\n                tableData: [],\n                editForm: {},\n                editRow:{},\n                dialogVisible: false,\n                newVisable:false,\n                newForm:{},\n                searchForm: {},\n                options: [\n                    {label: '正常', value: '0'},\n                    {label: '停用', value: '-1'}\n                ],\n                rules: {\n                    name:[\n                        {required: true, message: '名称不能为空', trigger: 'blur'},\n                    ]\n                }\n            }\n        },\n        methods: {\n            init() {\n                listByQo(this.searchForm).then(res => {\n                    if (res.data.code == 200) {\n                        this.tableData = res.data.data\n                    }else {\n                        popup(res.data.msg,\"error\")\n                    }\n\n                })\n            },\n            forbidden(id) {\n                deactivate(id).then(res => {\n                    res = res.data\n                    if (res.code == 200) {\n                        popup(\"操作成功\")\n                    } else {\n                        popup(res.msg, \"error\")\n                    }\n                    this.init()\n                })\n            },\n\n            /*弹出修改表单*/\n            edit(row) {\n                this.editForm= {...row}\n                this.dialogVisible = true\n            },\n            /*条件搜索*/\n            subSearchForm() {\n               this.init()\n            },\n            /*对接后端修改接口*/\n            submitEditForm() {\n                update(this.editForm).then(res=>{\n                    res=res.data\n                    if (res.code==200){\n                        popup(\"操作成功\")\n                        this.dialogVisible=false\n                        this.init()\n                    }else {\n                        popup(res.msg, \"error\")\n                    }\n                })\n            },\n            /*修改重置按钮*/\n            canCelEdit(){\n              this.dialogVisible=false\n                this.editForm={}\n            },\n            /*创建新角色*/\n            submitNewForm(formName){\n                this.$refs[formName].validate((valid) => {\n                    if (valid) {\n                        console.log(this.newForm)\n                        save(this.newForm).then(res=>{\n                            res=res.data\n                            if (res.code==200){\n                                popup(\"操作成功\")\n                                this.newVisable=false\n                                this.newForm={}\n                                this.init()\n                            }else {\n                                popup(res.msg, \"error\")\n                            }\n                        })\n                    }\n                });\n            },\n            resetNewForm(formname){\n                this.$refs[formname].resetFields()\n            },\n        },\n        created() {\n            this.init()\n        }\n\n    }\n</script>\n<style>\n    .search {\n        display: inline-block;\n        margin: 0 auto;\n        height: 51px;\n        line-height: 51px;\n    }\n\n    .search .column {\n        display: inline-block;\n        margin: 0 auto;\n        margin-right: 2px;\n        line-height: 48px;\n        height: 51px;\n    }\n</style>"],"mappings":"AAoIA,SAAAA,KAAA;AACA,SAAAC,UAAA,EAAAC,QAAA,EAAAC,IAAA,EAAAC,MAAA;AAEA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,QAAA;MACAC,OAAA;MACAC,aAAA;MACAC,UAAA;MACAC,OAAA;MACAC,UAAA;MACAC,OAAA,GACA;QAAAC,KAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,KAAA;QAAAC,KAAA;MAAA,EACA;MACAC,KAAA;QACAC,IAAA,GACA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAC,OAAA;IACAC,KAAA;MACApB,QAAA,MAAAU,UAAA,EAAAW,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAnB,IAAA,CAAAoB,IAAA;UACA,KAAAnB,SAAA,GAAAkB,GAAA,CAAAnB,IAAA,CAAAA,IAAA;QACA;UACAL,KAAA,CAAAwB,GAAA,CAAAnB,IAAA,CAAAqB,GAAA;QACA;MAEA;IACA;IACAC,UAAAC,EAAA;MACA3B,UAAA,CAAA2B,EAAA,EAAAL,IAAA,CAAAC,GAAA;QACAA,GAAA,GAAAA,GAAA,CAAAnB,IAAA;QACA,IAAAmB,GAAA,CAAAC,IAAA;UACAzB,KAAA;QACA;UACAA,KAAA,CAAAwB,GAAA,CAAAE,GAAA;QACA;QACA,KAAAJ,IAAA;MACA;IACA;IAEA;IACAO,KAAAC,GAAA;MACA,KAAAvB,QAAA;QAAA,GAAAuB;MAAA;MACA,KAAArB,aAAA;IACA;IACA;IACAsB,cAAA;MACA,KAAAT,IAAA;IACA;IACA;IACAU,eAAA;MACA5B,MAAA,MAAAG,QAAA,EAAAgB,IAAA,CAAAC,GAAA;QACAA,GAAA,GAAAA,GAAA,CAAAnB,IAAA;QACA,IAAAmB,GAAA,CAAAC,IAAA;UACAzB,KAAA;UACA,KAAAS,aAAA;UACA,KAAAa,IAAA;QACA;UACAtB,KAAA,CAAAwB,GAAA,CAAAE,GAAA;QACA;MACA;IACA;IACA;IACAO,WAAA;MACA,KAAAxB,aAAA;MACA,KAAAF,QAAA;IACA;IACA;IACA2B,cAAAC,QAAA;MACA,KAAAC,KAAA,CAAAD,QAAA,EAAAE,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACAC,OAAA,CAAAC,GAAA,MAAA7B,OAAA;UACAR,IAAA,MAAAQ,OAAA,EAAAY,IAAA,CAAAC,GAAA;YACAA,GAAA,GAAAA,GAAA,CAAAnB,IAAA;YACA,IAAAmB,GAAA,CAAAC,IAAA;cACAzB,KAAA;cACA,KAAAU,UAAA;cACA,KAAAC,OAAA;cACA,KAAAW,IAAA;YACA;cACAtB,KAAA,CAAAwB,GAAA,CAAAE,GAAA;YACA;UACA;QACA;MACA;IACA;IACAe,aAAAC,QAAA;MACA,KAAAN,KAAA,CAAAM,QAAA,EAAAC,WAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAtB,IAAA;EACA;AAEA"},"metadata":{},"sourceType":"module","externalDependencies":[]}