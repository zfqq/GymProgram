{"ast":null,"code":"import { popup } from \"@/assets/js/common\";\nimport { delMember, queryPageByQo, save, update, queryMemberById, getGoodsList, instructorsList } from \"@/api/member_management/member/memberApi\";\nimport { queryPointProductByGoodsId, queryPointProductBymemberId, saveExchangePointProductRecords } from \"@/api/sale_management/exchange_point_products/exchangePointProductsApi\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      BaseApi: this.$store.state.BaseApi,\n      newVisable: false,\n      editVisable: false,\n      searchForm: {\n        pageSize: 5\n      },\n      optionsk: [],\n      tableData: [],\n      instructorList: [],\n      newForm: {},\n      editForm: {},\n      instructor: {},\n      rules: {\n        name: [{\n          required: true,\n          message: \"会员姓名不能为空\",\n          trigger: \"blur\"\n        }],\n        phone: [{\n          required: true,\n          message: \"手机号不能为空\",\n          trigger: \"blur\"\n        }],\n        cardnumber: [{\n          required: true,\n          message: \"会员卡号不能为空\",\n          trigger: \"blur\"\n        }]\n      },\n      options_pointProducts: [],\n      pointProductsForm: {\n        goodsId: \"\",\n        coverUrl: \"\",\n        integral: \"\"\n      },\n      pointProductsVisable: false\n    };\n  },\n  methods: {\n    init() {\n      queryPageByQo(this.searchForm).then(res => {\n        res = res.data;\n        if (res.code == 200) {\n          console.log(res.data);\n          this.tableData = res.data.records;\n          this.searchForm.total = res.data.total;\n          this.searchForm.pageSize = res.data.size;\n          this.searchForm.currentPage = res.data.current;\n        } else {\n          popup(res.msg, \"error\");\n        }\n      });\n      getGoodsList().then(res => {\n        res = res.data;\n        if (res.code == 200) {\n          console.log(res.data);\n          this.optionsk = res.data;\n        }\n      });\n      instructorsList().then(res => {\n        res = res.data;\n        if (res.code == 200) {\n          console.log(res.data);\n          this.instructorList = res.data;\n        }\n      });\n    },\n    /*分页*/\n    handleSizeChange(val) {\n      this.searchForm.pageSize = val;\n      this.init();\n      console.log(`每页 ${val} 条`);\n    },\n    handleCurrentChange(val) {\n      this.searchForm.currentPage = val;\n      this.init();\n      console.log(`当前页: ${val}`);\n    },\n    submitSearchForm() {\n      this.init();\n    },\n    saveCancel(formName) {\n      this.newForm = {};\n      this.$refs[formName].resetFields();\n      this.newVisable = false;\n    },\n    submitNewForm(formName) {\n      this.$refs[formName].validate(valid => {\n        if (valid) {\n          this.newForm.course = this.newForm.course.join(',');\n          save(this.newForm).then(res => {\n            res = res.data;\n            if (res.code == 200) {\n              popup(\"操作成功\");\n              this.newForm = {};\n              this.newVisable = false;\n              this.init();\n            } else {\n              popup(res.msg, \"error\");\n            }\n          });\n        }\n      });\n    },\n    editBtn(id) {\n      queryMemberById({\n        id: id\n      }).then(res => {\n        res = res.data;\n        if (res.code == 200) {\n          this.editForm = {\n            ...res.data\n          };\n          this.editVisable = true;\n          instructor = res.data.instructor;\n        } else {\n          popup(res.msg, \"error\");\n        }\n      });\n    },\n    submitEditForm(formName) {\n      this.$refs[formName].validate(valid => {\n        if (valid) {\n          update(this.editForm).then(res => {\n            res = res.data;\n            if (res.code == 200) {\n              popup(\"操作成功\");\n              this.editForm = {};\n              this.editVisable = false;\n              this.init();\n            } else {\n              popup(res.msg, \"error\");\n            }\n          });\n        }\n      });\n    },\n    resetEditForm(formName) {\n      this.editForm = {};\n      this.$refs[formName].resetFields();\n      this.editVisable = false;\n    },\n    delBtn(id) {\n      this.$confirm(\"确定删除该会员？\", \"警示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(() => {\n        delMember({\n          id: id\n        }).then(res => {\n          res = res.data;\n          if (res.code == 200) {\n            popup(\"操作成功\");\n            this.searchForm.state = \"0\";\n            this.init();\n          } else {\n            popup(res.msg, \"error\");\n          }\n        });\n      }).catch(() => {\n        this.$message({\n          type: \"info\",\n          message: \"已取消操作\"\n        });\n      });\n    },\n    closePointProductsForm() {\n      this.options_pointProducts = [];\n      this.pointProductsForm = {};\n      this.pointProductsVisable = false;\n    },\n    queryPointProductByGoodsId(goodsId) {\n      queryPointProductByGoodsId({\n        goodsId: goodsId\n      }).then(res => {\n        res = res.data;\n        if (res.code == 200) {\n          if (!res.data) {\n            this.pointProductsForm = {\n              memberId: this.pointProductsForm.memberId,\n              goodsId: \"\",\n              coverUrl: \"\",\n              integral: \"\"\n            };\n            return;\n          }\n          this.pointProductsForm.integral = res.data.integral;\n          this.pointProductsForm.coverUrl = res.data.coverUrl;\n        } else {\n          popup(res.msg, \"error\");\n          this.pointProductsForm.goodsId = \"\";\n        }\n      });\n    }\n  },\n  mounted() {\n    /*初始化表格数据*/\n    this.searchForm.state = \"0\";\n    this.init();\n  }\n};","map":{"version":3,"names":["popup","delMember","queryPageByQo","save","update","queryMemberById","getGoodsList","instructorsList","queryPointProductByGoodsId","queryPointProductBymemberId","saveExchangePointProductRecords","axios","data","BaseApi","$store","state","newVisable","editVisable","searchForm","pageSize","optionsk","tableData","instructorList","newForm","editForm","instructor","rules","name","required","message","trigger","phone","cardnumber","options_pointProducts","pointProductsForm","goodsId","coverUrl","integral","pointProductsVisable","methods","init","then","res","code","console","log","records","total","size","currentPage","current","msg","handleSizeChange","val","handleCurrentChange","submitSearchForm","saveCancel","formName","$refs","resetFields","submitNewForm","validate","valid","course","join","editBtn","id","submitEditForm","resetEditForm","delBtn","$confirm","confirmButtonText","cancelButtonText","type","catch","$message","closePointProductsForm","memberId","mounted"],"sources":["src/views/member_management/member/List.vue"],"sourcesContent":["<template>\n    <div id=\"employee_index\">\n        <el-breadcrumb separator=\"/\">\n            <el-breadcrumb-item>首页</el-breadcrumb-item>\n            <el-breadcrumb-item>会员管理</el-breadcrumb-item>\n            <el-breadcrumb-item>会员信息</el-breadcrumb-item> </el-breadcrumb\n        ><br />\n\n        <el-row>\n            <el-col :span=\"8\" style=\"text-align: left; padding-right: 10px\">\n                <el-input placeholder=\"姓名\" v-model=\"searchForm.name\" />\n            </el-col>\n            <el-col :span=\"8\" style=\"text-align: left; padding-right: 10px\">\n                <el-input placeholder=\"手机号码\" v-model=\"searchForm.phone\" />\n            </el-col>\n            <el-col :span=\"8\">\n                <el-select\n                    v-model=\"searchForm.state\"\n                    @change=\"$forceUpdate()\"\n                    placeholder=\"状态\"\n                    clearable\n                >\n                    <el-option label=\"正常\" value=\"0\"></el-option>\n                    <el-option label=\"删除\" value=\"1\"></el-option>\n                </el-select>\n            </el-col>\n        </el-row>\n        <br />\n        <el-row>\n            <el-col :span=\"24\" style=\"text-align: left\">\n                <el-button\n                    type=\"primary\"\n                    @click=\"submitSearchForm\"\n                    style=\"font-size: 18px\"\n                >\n                    <i class=\"iconfont icon-r-find\" style=\"font-size: 18px\">\n                    </i>\n                    搜索</el-button\n                >\n                <el-button\n                    type=\"success\"\n                    @click=\"newVisable = true\"\n                    style=\"font-size: 18px\"\n                >\n                    <i class=\"iconfont icon-r-add\" style=\"font-size: 18px\"> </i>\n                    录入会员\n                </el-button>\n            </el-col>\n        </el-row>\n        <br />\n        <!--表格数据-->\n        <div class=\"table\">\n            <el-table :data=\"tableData\" style=\"width: 100%\" size=\"medium\">\n                <el-table-column prop=\"name\" label=\"姓名\"> </el-table-column>\n                <el-table-column prop=\"phone\" label=\"手机号\"> </el-table-column>\n                <el-table-column prop=\"cardnumber\" label=\"会员卡号\"> </el-table-column>\n                <el-table-column\n                    prop=\"email\"\n                    :show-overflow-tooltip=\"true\"\n                    label=\"邮箱\"\n                >\n                </el-table-column>\n                <el-table-column prop=\"integral\" label=\"积分\">\n                </el-table-column>\n                <el-table-column prop=\"state\" label=\"状态\">\n                    <template v-slot=\"scope\">\n                        <el-tag v-if=\"scope.row.state == '0'\" type=\"success\"\n                            >正常</el-tag\n                        >\n                        <el-tag v-else type=\"danger\">删除</el-tag>\n                    </template>\n                </el-table-column>\n                <el-table-column\n                    prop=\"info\"\n                    :show-overflow-tooltip=\"true\"\n                    label=\"备注\"\n                >\n                </el-table-column>\n                <el-table-column label=\"操作\" width=\"420\" fixed=\"right\">\n                    <template v-slot=\"scope\">\n                        <el-button\n                            type=\"success\"\n                            @click=\"editBtn(scope.row.id)\"\n                            style=\"font-size: 18px\"\n                        >\n                            <i\n                                class=\"iconfont icon-r-edit\"\n                                style=\"font-size: 18px\"\n                            ></i>\n                            修改</el-button\n                        >\n                        <el-button\n                            type=\"danger\"\n                            @click=\"delBtn(scope.row.id)\"\n                            style=\"font-size: 18px\"\n                        >\n                            <i\n                                class=\"iconfont icon-r-delete\"\n                                style=\"font-size: 18px\"\n                            ></i>\n                            删除</el-button\n                        >\n                    </template>\n                </el-table-column>\n            </el-table>\n            <div style=\"margin: 10px 0 15px 0\">\n                <el-pagination\n                    @size-change=\"handleSizeChange\"\n                    @current-change=\"handleCurrentChange\"\n                    :current-page.sync=\"searchForm.currentPage\"\n                    :page-sizes=\"[5, 10, 20, 50]\"\n                    :page-size=\"searchForm.pageSize\"\n                    layout=\"total,sizes, prev, pager, next,jumper\"\n                    :total=\"searchForm.total\"\n                >\n                </el-pagination>\n            </div>\n        </div>\n\n        <!--录入-->\n        <el-dialog\n            title=\"录入会员\"\n            :visible.sync=\"newVisable\"\n            label-width=\"200\"\n            width=\"50%\"\n        >\n            <el-form\n                :model=\"newForm\"\n                :rules=\"rules\"\n                ref=\"newForm\"\n                label-width=\"100px\"\n                class=\"demo-ruleForm\"\n            >\n                <el-form-item label=\"姓名：\" prop=\"name\">\n                    <el-input\n                        v-model=\"newForm.name\"\n                        placeholder=\"请输入姓名\"\n                    ></el-input>\n                </el-form-item>\n                 <el-form-item label=\"联系方式：\" prop=\"phone\">\n                    <el-input\n                        max=\"11\"\n                        placeholder=\"请输入手机号\"\n                        v-model=\"newForm.phone\"\n                    ></el-input>\n                </el-form-item>\n                <el-form-item label=\"会员卡号：\" prop=\"cardnumber\">\n                    <el-input\n                        v-model=\"newForm.cardnumber\"\n                        placeholder=\"请输入会员卡号\"\n                    ></el-input>\n                </el-form-item>\n\n                <el-form-item label=\"会员年龄：\" prop=\"age\">\n                    <el-input\n                        v-model=\"newForm.age\"\n                        placeholder=\"请输入会员卡号\"\n                    ></el-input>\n                </el-form-item>\n\n\n                <el-form-item label=\"会员性别：\" prop=\"sex\">\n                    <el-input\n                        v-model=\"newForm.sex\"\n                        placeholder=\"请输入会员卡号\"\n                    ></el-input>\n                </el-form-item>\n\n                \n                <el-form-item label=\"会员教练：\">\n                    <el-select v-model=\"newForm.instructor\" filterable  placeholder=\"请选择\">\n                        <el-option\n                        v-for=\"item in instructorList\"\n                        :key=\"item.nickName\"\n                        :label=\"item.nickName\"\n                        :value=\"item.id\">\n                        </el-option>\n                    </el-select>\n                </el-form-item>\n\n                <el-form-item label=\"会员总金额：\" prop=\"amount\">\n                    <el-input\n                        v-model=\"newForm.amount\"\n                        placeholder=\"请输入会员登记金额\"\n                    ></el-input>\n                </el-form-item>\n\n                <el-form-item label=\"备注：\">\n                    <el-input\n                        type=\"textarea\"\n                        v-model=\"newForm.info\"\n                        placeholder=\"如：新会员\"\n                    ></el-input>\n                </el-form-item>\n                <el-form-item label=\"课程选择：\">\n                    <el-select v-model=\"newForm.course\" multiple placeholder=\"请选择\">\n                        <el-option\n                        v-for=\"item in optionsk\"\n                        :key=\"item.name\"\n                        :label=\"item.name\"\n                        :value=\"item.id\">\n                        </el-option>\n                    </el-select>\n                </el-form-item>\n               \n\n                <el-form-item>\n                    <el-button\n                        type=\"primary\"\n                        @click=\"submitNewForm('newForm')\"\n                        style=\"font-size: 18px\"\n                    >\n                        <i\n                            class=\"iconfont icon-r-yes\"\n                            style=\"font-size: 18px\"\n                        ></i>\n                        提交</el-button\n                    >\n                    <el-button\n                        @click=\"saveCancel('newForm')\"\n                        style=\"font-size: 18px\"\n                    >\n                        取消</el-button\n                    >\n                </el-form-item>\n            </el-form>\n        </el-dialog>\n        <!--修改-->\n        <el-dialog\n            title=\"修改会员\"\n            :visible.sync=\"editVisable\"\n            size=\"mini\"\n            label-width=\"200\"\n            width=\"50%\"\n        >\n            <el-form\n                :model=\"editForm\"\n                :rules=\"rules\"\n                ref=\"editForm\"\n                label-width=\"100px\"\n                class=\"demo-ruleForm\"\n            >\n            <el-form-item label=\"姓名：\" prop=\"name\">\n                    <el-input\n                        v-model=\"editForm.name\"\n                        placeholder=\"请输入姓名\"\n                    ></el-input>\n                </el-form-item>\n                <el-form-item label=\"号码：\" prop=\"phone\">\n                    <el-input\n                        max=\"11\"\n                        placeholder=\"请输入手机号\"\n                        v-model=\"editForm.phone\"\n                    ></el-input>\n                </el-form-item>\n\n                <el-form-item label=\"会员卡号：\" prop=\"cardnumber\">\n                    <el-input\n                        v-model=\"editForm.cardnumber\"\n                        placeholder=\"请输入会员卡号\"\n                    ></el-input>\n                </el-form-item>\n                <el-form-item label=\"邮箱：\">\n                    <el-input\n                        v-model=\"editForm.email\"\n                        placeholder=\"如：xxx@qq.com\"\n                    ></el-input>\n                </el-form-item>\n\n                <el-form-item label=\"会员年龄：\" prop=\"age\">\n                    <el-input\n                        v-model=\"editForm.age\"\n                        placeholder=\"请输入会员卡号\"\n                    ></el-input>\n                </el-form-item>\n\n\n                <el-form-item label=\"会员性别：\" prop=\"sex\">\n                    <el-input\n                        v-model=\"editForm.sex\"\n                        placeholder=\"请输入会员卡号\"\n                    ></el-input>\n                </el-form-item>\n\n                \n                <el-form-item label=\"会员教练：\">\n                    <el-select v-model=\"editForm.instructor\" filterable  placeholder=\"请选择\">\n                        <el-option\n                        v-for=\"item in instructorList\"\n                        :key=\"item.nickName\"\n                        :label=\"item.nickName\"\n                        :value=\"item.id\">\n                        </el-option>\n                    </el-select>\n                </el-form-item>\n                \n              \n                <el-form-item label=\"备注：\">\n                    <el-input\n                        type=\"textarea\"\n                        v-model=\"editForm.info\"\n                        placeholder=\"如：新会员\"\n                    ></el-input>\n                </el-form-item>\n                <el-form-item label=\"状态：\" prop=\"state\">\n                    <el-select\n                        v-model=\"editForm.state\"\n                        placeholder=\"请选择状态\"\n                        filterable\n                        @change=\"$forceUpdate()\"\n                        clearable\n                    >\n                        <el-option label=\"正常\" value=\"0\"></el-option>\n                        <el-option label=\"删除\" value=\"1\"></el-option>\n                    </el-select>\n                </el-form-item>\n                <el-form-item>\n                    <el-button\n                        type=\"primary\"\n                        @click=\"submitEditForm('editForm')\"\n                        style=\"font-size: 18px\"\n                    >\n                        <i\n                            class=\"iconfont icon-r-yes\"\n                            style=\"font-size: 18px\"\n                        ></i>\n                        提交</el-button\n                    >\n                    <el-button\n                        type=\"info\"\n                        @click=\"resetEditForm('editForm')\"\n                        style=\"font-size: 18px\"\n                    >\n                        关闭</el-button\n                    >\n                </el-form-item>\n            </el-form>\n        </el-dialog>\n    </div>\n</template>\n<script>\nimport { popup } from \"@/assets/js/common\";\nimport {\n    delMember,\n    queryPageByQo,\n    save,\n    update,\n    queryMemberById,\n    getGoodsList,\n    instructorsList,\n} from \"@/api/member_management/member/memberApi\";\nimport {\n    queryPointProductByGoodsId,\n    queryPointProductBymemberId,\n    saveExchangePointProductRecords,\n} from \"@/api/sale_management/exchange_point_products/exchangePointProductsApi\";\nimport axios from \"axios\";\n\nexport default {\n    data() {\n        return {\n            BaseApi: this.$store.state.BaseApi,\n            newVisable: false,\n            editVisable: false,\n            searchForm: {\n                pageSize: 5,\n            },\n            optionsk: [],\n            tableData: [],\n            instructorList:[],\n            newForm: {},\n            editForm: {},\n            instructor:{},\n            rules: {\n                name: [\n                    {\n                        required: true,\n                        message: \"会员姓名不能为空\",\n                        trigger: \"blur\",\n                    },\n                ],\n                phone: [\n                    {\n                        required: true,\n                        message: \"手机号不能为空\",\n                        trigger: \"blur\",\n                    },\n                ],\n                cardnumber: [\n                    {\n                        required: true,\n                        message: \"会员卡号不能为空\",\n                        trigger: \"blur\",\n                    },\n                ],\n            },\n            options_pointProducts: [],\n            pointProductsForm: {\n                goodsId: \"\",\n                coverUrl: \"\",\n                integral: \"\",\n            },\n            pointProductsVisable: false,\n        };\n    },\n    methods: {\n        init() {\n            queryPageByQo(this.searchForm).then((res) => {\n                res = res.data;\n                if (res.code == 200) {\n                    console.log(res.data);\n                    this.tableData = res.data.records;\n                    this.searchForm.total = res.data.total;\n                    this.searchForm.pageSize = res.data.size;\n                    this.searchForm.currentPage = res.data.current;\n                } else {\n                    popup(res.msg, \"error\");\n                }\n            });\n            getGoodsList().then((res) => {\n                res = res.data;\n                if (res.code == 200) {\n                    console.log(res.data);\n                this.optionsk = res.data;\n                }\n                    });\n            instructorsList().then((res) => {\n                res = res.data;\n                if (res.code == 200) {\n                    console.log(res.data);\n                this.instructorList = res.data;\n                }\n                    });\n        },\n        /*分页*/\n        handleSizeChange(val) {\n            this.searchForm.pageSize = val;\n            this.init();\n            console.log(`每页 ${val} 条`);\n        },\n        handleCurrentChange(val) {\n            this.searchForm.currentPage = val;\n            this.init();\n            console.log(`当前页: ${val}`);\n        },\n        submitSearchForm() {\n            this.init();\n        },\n        saveCancel(formName) {\n            this.newForm = {};\n            this.$refs[formName].resetFields();\n            this.newVisable = false;\n        },\n        submitNewForm(formName) {\n            this.$refs[formName].validate((valid) => {\n                if (valid) {\n                    this.newForm.course=   this.newForm.course.join(',');\n                    save(this.newForm).then((res) => {\n                        res = res.data;\n                        if (res.code == 200) {\n                            popup(\"操作成功\");\n                            this.newForm = {};\n                            this.newVisable = false;\n                            this.init();\n                        } else {\n                            popup(res.msg, \"error\");\n                        }\n                    });\n                }\n            });\n        },\n        editBtn(id) {\n            queryMemberById({ id: id }).then((res) => {\n                res = res.data;\n                if (res.code == 200) {\n                    this.editForm = { ...res.data };\n                    this.editVisable = true;\n                    instructor =  res.data.instructor;\n                } else {\n                    popup(res.msg, \"error\");\n                }\n            });\n        },\n        submitEditForm(formName) {\n            this.$refs[formName].validate((valid) => {\n                if (valid) {\n                    update(this.editForm).then((res) => {\n                        res = res.data;\n                        if (res.code == 200) {\n                            popup(\"操作成功\");\n                            this.editForm = {};\n                            this.editVisable = false;\n                            this.init();\n                        } else {\n                            popup(res.msg, \"error\");\n                        }\n                    });\n                }\n            });\n        },\n        resetEditForm(formName) {\n            this.editForm = {};\n            this.$refs[formName].resetFields();\n            this.editVisable = false;\n        },\n        delBtn(id) {\n            this.$confirm(\"确定删除该会员？\", \"警示\", {\n                confirmButtonText: \"确定\",\n                cancelButtonText: \"取消\",\n                type: \"warning\",\n            })\n                .then(() => {\n                    delMember({ id: id }).then((res) => {\n                        res = res.data;\n                        if (res.code == 200) {\n                            popup(\"操作成功\");\n                            this.searchForm.state = \"0\";\n                            this.init();\n                        } else {\n                            popup(res.msg, \"error\");\n                        }\n                    });\n                })\n                .catch(() => {\n                    this.$message({\n                        type: \"info\",\n                        message: \"已取消操作\",\n                    });\n                });\n        },\n        closePointProductsForm() {\n            this.options_pointProducts = [];\n            this.pointProductsForm = {};\n            this.pointProductsVisable = false;\n        },\n        queryPointProductByGoodsId(goodsId) {\n            queryPointProductByGoodsId({ goodsId: goodsId }).then((res) => {\n                res = res.data;\n                if (res.code == 200) {\n                    if (!res.data) {\n                        this.pointProductsForm = {\n                            memberId: this.pointProductsForm.memberId,\n                            goodsId: \"\",\n                            coverUrl: \"\",\n                            integral: \"\",\n                        };\n                        return;\n                    }\n                    this.pointProductsForm.integral = res.data.integral;\n                    this.pointProductsForm.coverUrl = res.data.coverUrl;\n                } else {\n                    popup(res.msg, \"error\");\n                    this.pointProductsForm.goodsId = \"\";\n                }\n            });\n        },\n    },\n    mounted() {\n        /*初始化表格数据*/\n        this.searchForm.state = \"0\";\n        this.init();\n    },\n};\n</script>\n<style>\n.searchForm {\n    height: 134px;\n}\n\n.table {\n    height: 320px;\n}\n\n.pageUtils {\n    position: absolute;\n    width: 8