{"ast":null,"code":"import { popup } from \"@/assets/js/common\";\nimport { delSaleRecords, queryPageByQoSaleRecords } from \"@/api/sale_management/saleRecordsApi\";\nexport default {\n  data() {\n    return {\n      detailSaleRecordsVisable: false,\n      searchForm: {},\n      tableData: [],\n      saleRecordForm: {\n        cn: '',\n        sellway: '',\n        info: '',\n        sellTotal: '',\n        sellTotalmoney: '',\n        type: '0',\n        detailSaleRecords: []\n      }\n    };\n  },\n  methods: {\n    init() {\n      if (this.searchForm.startSellTime > this.searchForm.endSellTime) {\n        popup(\"结束时间不能小于开始时间\", \"warning\");\n        return;\n      }\n      queryPageByQoSaleRecords(this.searchForm).then(res => {\n        res = res.data;\n        if (res.code == 200) {\n          this.tableData = res.data.records;\n          this.searchForm.total = res.data.total;\n          this.searchForm.pageSize = res.data.size;\n          this.searchForm.currentPage = res.data.current;\n        } else {\n          popup(res.msg, \"error\");\n        }\n      });\n    },\n    del(cn) {\n      this.$confirm('确定删除这条记录？', '警示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        delSaleRecords({\n          cn: cn\n        }).then(res => {\n          res = res.data;\n          if (res.code == 200) {\n            popup(\"操作成功\");\n            this.init();\n          } else {\n            popup(res.msg, \"error\");\n          }\n        });\n      }).catch(() => {\n        this.$message({\n          type: 'info',\n          message: '已取消操作'\n        });\n      });\n    },\n    detailBtn(row) {\n      this.saleRecordForm = {\n        cn: '',\n        sellway: '',\n        info: '',\n        sellTotal: '',\n        sellTotalmoney: '',\n        type: '0',\n        detailSaleRecords: []\n      };\n      this.saleRecordForm = {\n        ...row\n      };\n      this.detailSaleRecordsVisable = true;\n    },\n    /*分页*/\n    handleSizeChange(val) {\n      this.searchForm.pageSize = val;\n      this.init();\n      console.log(`每页 ${val} 条`);\n    },\n    handleCurrentChange(val) {\n      this.searchForm.currentPage = val;\n      this.init();\n      console.log(`当前页: ${val}`);\n    },\n    submitSearchForm() {\n      this.init();\n    }\n  },\n  mounted() {\n    this.init();\n  }\n};","map":{"version":3,"names":["popup","delSaleRecords","queryPageByQoSaleRecords","data","detailSaleRecordsVisable","searchForm","tableData","saleRecordForm","cn","sellway","info","sellTotal","sellTotalmoney","type","detailSaleRecords","methods","init","startSellTime","endSellTime","then","res","code","records","total","pageSize","size","currentPage","current","msg","del","$confirm","confirmButtonText","cancelButtonText","catch","$message","message","detailBtn","row","handleSizeChange","val","console","log","handleCurrentChange","submitSearchForm","mounted"],"sources":["src/views/sale_management/sale_records/List.vue"],"sourcesContent":["<template>\n    <div id=\"sale_records\">\n        <el-breadcrumb separator=\"/\">\n            <el-breadcrumb-item :to=\"{ path: '/index' }\">首页</el-breadcrumb-item>\n            <el-breadcrumb-item :to=\"{path:'/sale_management/sale_cmd/list'}\">销售管理</el-breadcrumb-item>\n            <el-breadcrumb-item>销售记录</el-breadcrumb-item>\n        </el-breadcrumb>\n        <div class=\"searchForm\">\n            <div class=\"column\">\n                <span>编号：</span>\n                <el-input style=\"height: 21px;width: 130px\" placeholder=\"请输入销售编号\" v-model=\"searchForm.cn\"/>\n            </div>\n            <div class=\"column\">\n                <span>销售时间：</span>\n                <el-date-picker\n                        style=\"width: 140px\"\n                        v-model=\"searchForm.startSellTime\"\n                        type=\"date\"\n                        value-format=\"yyyy-MM-dd\"\n                        placeholder=\"起始时间\">\n                </el-date-picker>\n                -\n                <el-date-picker\n                        v-model=\"searchForm.endSellTime\"\n                        style=\"width: 140px\"\n                        value-format=\"yyyy-MM-dd\"\n                        type=\"date\"\n                        placeholder=\"结束时间\">\n                </el-date-picker>\n            </div>\n            <div class=\"column\">\n                <span>顾客类型：</span>\n                <el-select style=\"width: 120px\"\n                           v-model=\"searchForm.type\"\n                           @change=\"$forceUpdate()\"\n                           placeholder=\"请选择顾客类型\" clearable>\n                    <el-option label=\"非会员顾客\" value=\"0\"></el-option>\n                    <el-option label=\"会员\" value=\"1\"></el-option>\n                </el-select>\n            </div>\n            <div class=\"column\">\n                <span>支付方式：</span>\n                <el-select style=\"width: 120px\"\n                           v-model=\"searchForm.sellway\"\n                           @change=\"$forceUpdate()\"\n                           placeholder=\"请选择支付方式\" clearable>\n                    <el-option label=\"支付宝\" value=\"0\"></el-option>\n                    <el-option label=\"微信\" value=\"1\"></el-option>\n                    <el-option label=\"现金\" value=\"2\"></el-option>\n                    <el-option label=\"银行卡\" value=\"3\"></el-option>\n                </el-select>\n            </div>\n            <div class=\"column\">\n                <el-button type=\"success\" @click=\"submitSearchForm\">搜索</el-button>\n            </div>\n        </div>\n        <div class=\"table\">\n            <el-table\n                    :data=\"tableData\"\n                    style=\"width: 100%;\"\n                    height=\"450px\"\n                    size=\"medium\">\n                <el-table-column\n                        prop=\"cn\"\n                        width=\"200\"\n                        label=\"编号\">\n                </el-table-column>\n                <el-table-column\n                        prop=\"sellTotal\"\n                        label=\"销售数量\">\n                </el-table-column>\n                <el-table-column\n                        prop=\"sellTotalmoney\"\n                        label=\"销售总额\">\n                    <template v-slot=\"scope\">\n                        {{parseFloat(scope.row.sellTotalmoney).toFixed(2)}}\n                    </template>\n                </el-table-column>\n                <el-table-column\n                        prop=\"sellTime\"\n                        :show-overflow-tooltip=\"true\"\n                        label=\"销售时间\">\n                </el-table-column>\n                <el-table-column\n                        prop=\"type\"\n                        label=\"顾客类型\">\n                    <template v-slot=\"scope\">\n                        <el-tag effect=\"dark\" type=\"danger\" v-if=\"scope.row.type=='1'\">会员</el-tag>\n                        <el-tag effect=\"dark\" type=\"success\" v-else>非会员</el-tag>\n                    </template>\n                </el-table-column>\n                <el-table-column\n                        prop=\"sellby\"\n                        label=\"售卖者\">\n                </el-table-column>\n                <el-table-column\n                        prop=\"sellway\"\n                        label=\"支付方式\">\n                    <template v-slot=\"scope\">\n                        <el-tag effect=\"dark\" type=\"danger\" v-if=\"scope.row.sellway=='0'\">支付宝</el-tag>\n                        <el-tag effect=\"dark\" type=\"warning\" v-else-if=\"scope.row.sellway=='1'\">微信</el-tag>\n                        <el-tag effect=\"dark\" type=\"success\" v-else-if=\"scope.row.sellway=='3'\">银行卡</el-tag>\n                        <el-tag effect=\"dark\" type=\"info\" v-else>现金</el-tag>\n                    </template>\n                </el-table-column>\n                <el-table-column\n                        prop=\"info\"\n                        :show-overflow-tooltip=\"true\"\n                        label=\"备注\">\n                </el-table-column>\n                <el-table-column\n                        width=\"400\"\n                        label=\"操作\">\n                    <template v-slot=\"scope\">\n                        <el-button type=\"success\"\n                                   @click=\"detailBtn(scope.row)\" plain>明细\n                        </el-button>\n                        <el-button type=\"danger\"\n                                   @click=\"del(scope.row.cn)\" plain>删除\n                        </el-button>\n                    </template>\n                </el-table-column>\n            </el-table>\n            <div class=\"pageUtils\">\n                <el-pagination\n                        @size-change=\"handleSizeChange\"\n                        @current-change=\"handleCurrentChange\"\n                        :current-page.sync=\"searchForm.currentPage\"\n                        :page-sizes=\"[3, 5, 7, 9]\"\n                        :page-size=\"searchForm.pageSize\"\n                        layout=\"total,sizes, prev, pager, next,jumper\"\n                        :total=\"searchForm.total\">\n                </el-pagination>\n            </div>\n        </div>\n        <el-dialog\n                title=\"销售订单明细\"\n                :visible.sync=\"detailSaleRecordsVisable\"\n\n                width=\"80%\">\n            <el-form :model=\"saleRecordForm\"  label-width=\"100px\"\n                     class=\"demo-ruleForm\">\n                    <el-row>\n                        <el-col :span=\"12\">\n                            <el-form-item style=\"width: 60%\" label=\"销售编号：\">\n                                <el-input readonly v-model=\"saleRecordForm.cn\" placeholder=\"如：销售编号\"></el-input>\n                            </el-form-item>\n                        </el-col>\n                    </el-row>\n                    <el-row>\n                        <el-col  :span=\"12\">\n                            <el-form-item style=\"width: 80%\" label=\"支付方式：\" prop=\"sellway\">\n                                <el-radio disabled type=\"radio\" v-model=\"saleRecordForm.sellway\" label=\"0\">支付宝</el-radio>\n                                <el-radio disabled type=\"radio\" v-model=\"saleRecordForm.sellway\" label=\"1\">微信</el-radio>\n                                <el-radio disabled type=\"radio\" v-model=\"saleRecordForm.sellway\" label=\"2\">现金</el-radio>\n                                <el-radio disabled type=\"radio\" v-model=\"saleRecordForm.sellway\" label=\"3\">银行卡</el-radio>\n                            </el-form-item>\n                        </el-col>\n                    </el-row>\n                    <el-row>\n                        <el-col :span=\"12\">\n\n                            <el-form-item style=\"width:60%\" label=\"顾客类型：\">\n                                <el-radio disabled  type=\"radio\" v-model=\"saleRecordForm.type\" label=\"1\">会员</el-radio>\n                                <el-radio disabled  type=\"radio\" v-model=\"saleRecordForm.type\" label=\"0\">非会员</el-radio>\n                            </el-form-item>\n                        </el-col>\n                        <el-col :span=\"12\" v-if=\"saleRecordForm.type=='1'\" >\n                            <el-form-item style=\"width: 60%\" label=\"会员账号：\" prop=\"memberPhone\">\n                                <el-input readonly v-model=\"saleRecordForm.memberPhone\" placeholder=\"如：电话号码\"></el-input>\n                            </el-form-item>\n                        </el-col>\n                    </el-row>\n\n                    <el-row>\n                        <el-col :span=\"24\">\n                            <el-table\n                                    :data=\"saleRecordForm.detailSaleRecords\"\n                                    style=\"width: 100%;\"\n                                    height=\"450px\"\n                                    size=\"medium\">\n                                <el-table-column\n                                        prop=\"goodsId\"\n                                        label=\"商品编号\">\n                                </el-table-column>\n                                <el-table-column\n                                        prop=\"goodsName\"\n                                        label=\"商品名\">\n                                </el-table-column>\n                                <el-table-column\n                                        prop=\"goodsNum\"\n                                        label=\"数量\">\n                                    <template v-slot=\"scope\">\n                                        <el-button type=\"info\">+</el-button>\n                                        <el-input readonly style=\"width: 100px;\" type=\"number\" min=\"1\"\n                                                  v-model=\"scope.row.goodsNum\"></el-input>\n                                        <el-button type=\"info\">-</el-button>\n                                    </template>\n                                </el-table-column>\n                                <el-table-column\n                                        prop=\"goodsPrice\"\n                                        label=\"商品单价\">\n                                </el-table-column>\n                            </el-table>\n                        </el-col>\n                    </el-row>\n                    <el-row>\n                        <el-col :span=\"24\">\n                            <el-form-item style=\"width: 60%\" label=\"备注：\">\n                                <el-input readonly type=\"textarea\" v-model=\"saleRecordForm.info\" placeholder=\"如：订单1\"></el-input>\n                            </el-form-item>\n                        </el-col>\n                    </el-row>\n                    <el-row>\n                        <el-col :span=\"24\">\n                            <div v-if=\"saleRecordForm.detailSaleRecords.length>0\">\n                                订单号：{{saleRecordForm.cn}}&nbsp;&nbsp;\n                                商品总数：{{saleRecordForm.sellTotal?saleRecordForm.sellTotal:0}}&nbsp;&nbsp;\n                                消费：{{saleRecordForm.sellTotalmoney?parseFloat(saleRecordForm.sellTotalmoney).toFixed(2):0}}元&nbsp;&nbsp;\n                                <span v-if=\"saleRecordForm.sellway\">支付方式：{{saleRecordForm.sellway=='0'?'支付宝':saleRecordForm.sellway=='1'?'微信':saleRecordForm.sellway=='2'?'现金':'银行卡'}}&nbsp;&nbsp;</span>\n                                <span v-if=\"saleRecordForm.type=='1'\">会员享受9折&nbsp;&nbsp;</span>\n                                <span v-if=\"saleRecordForm.sellTime\">消费时间：{{saleRecordForm.sellTime}}&nbsp;&nbsp; 操作者编号：{{saleRecordForm.eid}}</span>\n                            </div>\n                        </el-col>\n                    </el-row>\n                    <el-row>\n                        <el-col :span=\"24\">\n                            <el-form-item>\n                                <el-button type=\"info\" @click=\"detailSaleRecordsVisable=false\">关闭</el-button>\n                            </el-form-item>\n                        </el-col>\n                    </el-row>\n                </el-form>\n        </el-dialog>\n\n\n\n\n    </div>\n</template>\n<script>\n    import {popup} from \"@/assets/js/common\";\n    import {delSaleRecords, queryPageByQoSaleRecords} from \"@/api/sale_management/saleRecordsApi\";\n\n    export default {\n        data() {\n            return {\n                detailSaleRecordsVisable:false,\n                searchForm: {},\n                tableData: [],\n                saleRecordForm:{\n                    cn: '',\n                    sellway: '',\n                    info: '',\n                    sellTotal: '',\n                    sellTotalmoney: '',\n                    type: '0',\n                    detailSaleRecords: []\n                }\n            }\n        },\n        methods: {\n            init() {\n                if (this.searchForm.startSellTime > this.searchForm.endSellTime) {\n                    popup(\"结束时间不能小于开始时间\", \"warning\")\n                    return;\n                }\n                queryPageByQoSaleRecords(this.searchForm).then(res => {\n                    res = res.data\n                    if (res.code == 200) {\n                        this.tableData = res.data.records\n                        this.searchForm.total = res.data.total\n                        this.searchForm.pageSize = res.data.size\n                        this.searchForm.currentPage = res.data.current\n                    } else {\n                        popup(res.msg, \"error\")\n                    }\n                })\n            },\n            del(cn) {\n                this.$confirm('确定删除这条记录？', '警示', {\n                    confirmButtonText: '确定',\n                    cancelButtonText: '取消',\n                    type: 'warning'\n                }).then(() => {\n                    delSaleRecords({cn: cn}).then(res => {\n                        res = res.data\n                        if (res.code == 200) {\n                            popup(\"操作成功\")\n                            this.init()\n                        } else {\n                            popup(res.msg, \"error\")\n                        }\n                    })\n                }).catch(() => {\n                    this.$message({\n                        type: 'info',\n                        message: '已取消操作'\n                    });\n                });\n            },\n            detailBtn(row){\n                this.saleRecordForm={\n                    cn: '',\n                    sellway: '',\n                    info: '',\n                    sellTotal: '',\n                    sellTotalmoney: '',\n                    type: '0',\n                    detailSaleRecords: []\n                }\n                this.saleRecordForm={...row}\n                this.detailSaleRecordsVisable=true\n            },\n            /*分页*/\n            handleSizeChange(val) {\n                this.searchForm.pageSize = val\n                this.init()\n                console.log(`每页 ${val} 条`);\n            },\n            handleCurrentChange(val) {\n                this.searchForm.currentPage = val\n                this.init()\n                console.log(`当前页: ${val}`);\n            },\n            submitSearchForm() {\n                this.init()\n            },\n        },\n        mounted() {\n            this.init()\n\n        }\n    }\n</script>\n<style>\n    .searchForm {\n        height: 134px;\n    }\n\n    .table {\n        background-color: #303133;\n        height: 320px;\n    }\n\n    .pageUtils {\n        position: absolute;\n        width: 84%;\n        margin: 2px auto;\n        height: 43px;\n    }\n\n    .searchForm {\n        display: inline-block;\n        margin: 0 auto;\n        height: 51px;\n        line-height: 51px;\n    }\n\n    .searchForm .column {\n        display: inline-block;\n        margin: 0 auto;\n        margin-right: 2px;\n        line-height: 48px;\n        height: 51px;\n    }\n</style>"],"mappings":"AAiPA,SAAAA,KAAA;AACA,SAAAC,cAAA,EAAAC,wBAAA;AAEA;EACAC,KAAA;IACA;MACAC,wBAAA;MACAC,UAAA;MACAC,SAAA;MACAC,cAAA;QACAC,EAAA;QACAC,OAAA;QACAC,IAAA;QACAC,SAAA;QACAC,cAAA;QACAC,IAAA;QACAC,iBAAA;MACA;IACA;EACA;EACAC,OAAA;IACAC,KAAA;MACA,SAAAX,UAAA,CAAAY,aAAA,QAAAZ,UAAA,CAAAa,WAAA;QACAlB,KAAA;QACA;MACA;MACAE,wBAAA,MAAAG,UAAA,EAAAc,IAAA,CAAAC,GAAA;QACAA,GAAA,GAAAA,GAAA,CAAAjB,IAAA;QACA,IAAAiB,GAAA,CAAAC,IAAA;UACA,KAAAf,SAAA,GAAAc,GAAA,CAAAjB,IAAA,CAAAmB,OAAA;UACA,KAAAjB,UAAA,CAAAkB,KAAA,GAAAH,GAAA,CAAAjB,IAAA,CAAAoB,KAAA;UACA,KAAAlB,UAAA,CAAAmB,QAAA,GAAAJ,GAAA,CAAAjB,IAAA,CAAAsB,IAAA;UACA,KAAApB,UAAA,CAAAqB,WAAA,GAAAN,GAAA,CAAAjB,IAAA,CAAAwB,OAAA;QACA;UACA3B,KAAA,CAAAoB,GAAA,CAAAQ,GAAA;QACA;MACA;IACA;IACAC,IAAArB,EAAA;MACA,KA