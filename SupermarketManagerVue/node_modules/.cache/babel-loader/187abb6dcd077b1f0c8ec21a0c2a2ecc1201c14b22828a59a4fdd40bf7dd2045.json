{"ast":null,"code":"import { popup } from \"@/assets/js/common\";\nimport { delSaleRecords, queryPageByQoSaleRecords } from \"@/api/sale_management/saleRecordsApi\";\nexport default {\n  data() {\n    return {\n      detailSaleRecordsVisable: false,\n      searchForm: {},\n      tableData: [],\n      saleRecordForm: {\n        cn: \"\",\n        sellway: \"\",\n        info: \"\",\n        sellTotal: \"\",\n        sellTotalmoney: \"\",\n        type: \"0\",\n        detailSaleRecords: []\n      }\n    };\n  },\n  methods: {\n    init() {\n      if (this.searchForm.startSellTime > this.searchForm.endSellTime) {\n        popup(\"结束时间不能小于开始时间\", \"warning\");\n        return;\n      }\n      queryPageByQoSaleRecords(this.searchForm).then(res => {\n        res = res.data;\n        if (res.code == 200) {\n          this.tableData = res.data.records;\n          this.searchForm.total = res.data.total;\n          this.searchForm.pageSize = res.data.size;\n          this.searchForm.currentPage = res.data.current;\n        } else {\n          popup(res.msg, \"error\");\n        }\n      });\n    },\n    del(cn) {\n      this.$confirm(\"确定删除这条记录？\", \"警示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(() => {\n        delSaleRecords({\n          cn: cn\n        }).then(res => {\n          res = res.data;\n          if (res.code == 200) {\n            popup(\"操作成功\");\n            this.init();\n          } else {\n            popup(res.msg, \"error\");\n          }\n        });\n      }).catch(() => {\n        this.$message({\n          type: \"info\",\n          message: \"已取消操作\"\n        });\n      });\n    },\n    detailBtn(row) {\n      this.saleRecordForm = {\n        cn: \"\",\n        sellway: \"\",\n        info: \"\",\n        sellTotal: \"\",\n        sellTotalmoney: \"\",\n        type: \"0\",\n        detailSaleRecords: []\n      };\n      this.saleRecordForm = {\n        ...row\n      };\n      this.detailSaleRecordsVisable = true;\n    },\n    /*分页*/\n    handleSizeChange(val) {\n      this.searchForm.pageSize = val;\n      this.init();\n      console.log(`每页 ${val} 条`);\n    },\n    handleCurrentChange(val) {\n      this.searchForm.currentPage = val;\n      this.init();\n      console.log(`当前页: ${val}`);\n    },\n    submitSearchForm() {\n      this.init();\n    }\n  },\n  mounted() {\n    this.init();\n  }\n};","map":{"version":3,"names":["popup","delSaleRecords","queryPageByQoSaleRecords","data","detailSaleRecordsVisable","searchForm","tableData","saleRecordForm","cn","sellway","info","sellTotal","sellTotalmoney","type","detailSaleRecords","methods","init","startSellTime","endSellTime","then","res","code","records","total","pageSize","size","currentPage","current","msg","del","$confirm","confirmButtonText","cancelButtonText","catch","$message","message","detailBtn","row","handleSizeChange","val","console","log","handleCurrentChange","submitSearchForm","mounted"],"sources":["src/views/sale_management/sale_records/List.vue"],"sourcesContent":["<template>\n    <div id=\"sale_records\">\n        <el-breadcrumb separator=\"/\">\n            <el-breadcrumb-item :to=\"{ path: '/index' }\"\n                >首页</el-breadcrumb-item\n            >\n            <el-breadcrumb-item :to=\"{ path: '/sale_management/sale_cmd/list' }\"\n                >销售管理</el-breadcrumb-item\n            >\n            <el-breadcrumb-item>销售记录</el-breadcrumb-item>\n        </el-breadcrumb>\n        <br />\n        <el-row>\n            <el-col :span=\"4\">\n                <el-input\n                    style=\"height: 21px; width: 130px\"\n                    placeholder=\"销售编号\"\n                    v-model=\"searchForm.cn\"\n                />\n            </el-col>\n            <el-col :span=\"10\">\n                <span>销售时间：</span>\n                <el-date-picker\n                    style=\"width: 140px\"\n                    v-model=\"searchForm.startSellTime\"\n                    type=\"date\"\n                    value-format=\"yyyy-MM-dd\"\n                    placeholder=\"起始时间\"\n                >\n                </el-date-picker>\n                -\n                <el-date-picker\n                    v-model=\"searchForm.endSellTime\"\n                    style=\"width: 140px\"\n                    value-format=\"yyyy-MM-dd\"\n                    type=\"date\"\n                    placeholder=\"结束时间\"\n                >\n                </el-date-picker>\n            </el-col>\n            <el-col :span=\"4\">\n                <el-select\n                    style=\"width: 120px\"\n                    v-model=\"searchForm.type\"\n                    @change=\"$forceUpdate()\"\n                    placeholder=\"顾客类型\"\n                    clearable\n                >\n                    <el-option label=\"非会员顾客\" value=\"0\"></el-option>\n                    <el-option label=\"会员\" value=\"1\"></el-option>\n                </el-select>\n            </el-col>\n            <el-col :span=\"4\">\n                <el-select\n                    style=\"width: 120px\"\n                    v-model=\"searchForm.sellway\"\n                    @change=\"$forceUpdate()\"\n                    placeholder=\"支付方式\"\n                    clearable\n                >\n                    <el-option label=\"支付宝\" value=\"0\"></el-option>\n                    <el-option label=\"微信\" value=\"1\"></el-option>\n                    <el-option label=\"现金\" value=\"2\"></el-option>\n                    <el-option label=\"银行卡\" value=\"3\"></el-option>\n                </el-select>\n            </el-col>\n        </el-row>\n        <br>\n        <el-row>\n            <el-col :span=\"24\" style=\"text-align: left;\">\n                \n                <el-button type=\"success\" @click=\"submitSearchForm\" style=\"font-size: 18px;\"\n                    >\n                    <i class=\"iconfont icon-r-find\" style=\"font-size: 18px;\"> </i>\n                    搜索</el-button\n                >\n            </el-col>\n        </el-row>\n        <br />\n        <div class=\"table\">\n            <el-table :data=\"tableData\" style=\"width: 100%\" size=\"medium\">\n                <el-table-column prop=\"cn\" width=\"200\" label=\"编号\">\n                </el-table-column>\n                <el-table-column prop=\"sellTotal\" label=\"销售数量\">\n                </el-table-column>\n                <el-table-column prop=\"sellTotalmoney\" label=\"销售总额\">\n                    <template v-slot=\"scope\">\n                        {{ parseFloat(scope.row.sellTotalmoney).toFixed(2) }}\n                    </template>\n                </el-table-column>\n                <el-table-column\n                    prop=\"sellTime\"\n                    :show-overflow-tooltip=\"true\"\n                    label=\"销售时间\"\n                >\n                </el-table-column>\n                <el-table-column prop=\"type\" label=\"顾客类型\" width=\"120\">\n                    <template v-slot=\"scope\">\n                        <el-tag\n                            effect=\"dark\"\n                            type=\"warning\"\n                            style=\"font-size: 14px;\"\n                            v-if=\"scope.row.type == '1'\"\n                            >\n                            <i class=\"iconfont icon-r-mark1\"></i>\n                            会员</el-tag\n                        >\n                        <el-tag effect=\"dark\" type=\"info\" v-else\n                            >非会员</el-tag\n                        >\n                    </template>\n                </el-table-column>\n                <el-table-column prop=\"sellby\" label=\"销售员\">\n                </el-table-column>\n                <el-table-column prop=\"sellway\" label=\"支付方式\">\n                    <template v-slot=\"scope\">\n                        <el-tag\n                            effect=\"dark\"\n                            type=\"primary\"\n                            v-if=\"scope.row.sellway == '0'\"\n                            >支付宝</el-tag\n                        >\n                        <el-tag\n                            effect=\"dark\"\n                            type=\"success\"\n                            v-else-if=\"scope.row.sellway == '1'\"\n                            >微信</el-tag\n                        >\n                        <el-tag\n                            effect=\"dark\"\n                            type=\"warning\"\n                            v-else-if=\"scope.row.sellway == '3'\"\n                            >银行卡</el-tag\n                        >\n                        <el-tag effect=\"dark\" type=\"danger\" v-else>现金</el-tag>\n                    </template>\n                </el-table-column>\n                <el-table-column\n                    prop=\"info\"\n                    :show-overflow-tooltip=\"true\"\n                    label=\"备注\"\n                >\n                </el-table-column>\n                <el-table-column width=\"240\" label=\"操作\" fixed=\"right\">\n                    <template v-slot=\"scope\">\n                        <el-button\n                            type=\"success\"\n                            style=\"font-size: 18px;\"\n                            @click=\"detailBtn(scope.row)\"\n                            >\n                            <i class=\"iconfont icon-r-find\" style=\"font-size: 18px;\"> </i>\n                            明细\n                        </el-button>\n                        <el-button\n                            type=\"danger\"\n                            @click=\"del(scope.row.cn)\"\n                            style=\"font-size: 18px;\"\n                            >\n                            <i class=\"iconfont icon-r-delete\" style=\"font-size: 18px;\"> </i>\n                            删除\n                        </el-button>\n                    </template>\n                </el-table-column>\n            </el-table>\n            <div class=\"pageUtils\">\n                <el-pagination\n                    @size-change=\"handleSizeChange\"\n                    @current-change=\"handleCurrentChange\"\n                    :current-page.sync=\"searchForm.currentPage\"\n                    :page-sizes=\"[3, 5, 7, 9]\"\n                    :page-size=\"searchForm.pageSize\"\n                    layout=\"total,sizes, prev, pager, next,jumper\"\n                    :total=\"searchForm.total\"\n                >\n                </el-pagination>\n            </div>\n        </div>\n        <el-dialog\n            title=\"销售订单明细\"\n            :visible.sync=\"detailSaleRecordsVisable\"\n            width=\"80%\"\n        >\n            <el-form\n                :model=\"saleRecordForm\"\n                label-width=\"100px\"\n                class=\"demo-ruleForm\"\n            >\n                <el-row>\n                    <el-col :span=\"12\">\n                        <el-form-item style=\"width: 60%\" label=\"销售编号：\">\n                            <el-input\n                            disabled\n                                readonly\n                                v-model=\"saleRecordForm.cn\"\n                                placeholder=\"如：销售编号\"\n                            ></el-input>\n                        </el-form-item>\n                    </el-col>\n                </el-row>\n                <el-row>\n                    <el-col :span=\"12\">\n                        <el-form-item\n                            style=\"width: 80%\"\n                            label=\"支付方式：\"\n                            prop=\"sellway\"\n                        >\n                            <el-radio\n                                disabled\n                                type=\"radio\"\n                                v-model=\"saleRecordForm.sellway\"\n                                label=\"0\"\n                                >支付宝</el-radio\n                            >\n                            <el-radio\n                                disabled\n                                type=\"radio\"\n                                v-model=\"saleRecordForm.sellway\"\n                                label=\"1\"\n                                >微信</el-radio\n                            >\n                            <el-radio\n                                disabled\n                                type=\"radio\"\n                                v-model=\"saleRecordForm.sellway\"\n                                label=\"2\"\n                                >现金</el-radio\n                            >\n                            <el-radio\n                                disabled\n                                type=\"radio\"\n                                v-model=\"saleRecordForm.sellway\"\n                                label=\"3\"\n                                >银行卡</el-radio\n                            >\n                        </el-form-item>\n                    </el-col>\n                </el-row>\n                <el-row>\n                    <el-col :span=\"12\">\n                        <el-form-item style=\"width: 60%\" label=\"顾客类型：\">\n                            <el-radio\n                                disabled\n                                type=\"radio\"\n                                v-model=\"saleRecordForm.type\"\n                                label=\"1\"\n                                >会员</el-radio\n                            >\n                            <el-radio\n                                disabled\n                                type=\"radio\"\n                                v-model=\"saleRecordForm.type\"\n                                label=\"0\"\n                                >非会员</el-radio\n                            >\n                        </el-form-item>\n                    </el-col>\n                    <el-col :span=\"12\" v-if=\"saleRecordForm.type == '1'\">\n                        <el-form-item\n                            style=\"width: 60%\"\n                            label=\"会员账号：\"\n                            prop=\"memberPhone\"\n                        >\n                            <el-input\n                                readonly\n                                v-model=\"saleRecordForm.memberPhone\"\n                                placeholder=\"如：电话号码\"\n                            ></el-input>\n                        </el-form-item>\n                    </el-col>\n                </el-row>\n                <hr>\n                <el-row>\n                    <el-col :span=\"24\">\n                        <el-table\n                            :data=\"saleRecordForm.detailSaleRecords\"\n                            style=\"width: 100%\"\n                            size=\"medium\"\n                        >\n                            <el-table-column prop=\"goodsId\" label=\"商品编号\">\n                            </el-table-column>\n                            <el-table-column prop=\"goodsName\" label=\"商品名\">\n                            </el-table-column>\n                            <el-table-column prop=\"goodsNum\" label=\"数量\">\n                                <template v-slot=\"scope\">\n                                    <el-button type=\"info\" disabled>-</el-button>\n                                    <el-input\n                                        readonly\n                                        style=\"width: 100px\"\n                                        type=\"number\"\n                                        min=\"1\"\n                                        v-model=\"scope.row.goodsNum\"\n                                    ></el-input>\n                                    <el-button type=\"info\" disabled>+</el-button>\n                                </template>\n                            </el-table-column>\n                            <el-table-column prop=\"goodsPrice\" label=\"商品单价\">\n                            </el-table-column>\n                        </el-table>\n                    </el-col>\n                </el-row>\n                <hr>\n                <el-row>\n                    <el-col :span=\"24\">\n                        <el-form-item style=\"width: 100%\" label=\"备注：\">\n                            <el-input\n                                disabled\n                                readonly\n                                type=\"textarea\"\n                                v-model=\"saleRecordForm.info\"\n                                placeholder=\"如：订单1\"\n                            ></el-input>\n                        </el-form-item>\n                    </el-col>\n                </el-row>\n                <el-row>\n                    <el-col :span=\"24\">\n                        <div v-if=\"saleRecordForm.detailSaleRecords.length > 0\">\n                            订单号：{{ saleRecordForm.cn }}&nbsp;&nbsp;\n                            商品总数：{{\n                                saleRecordForm.sellTotal\n                                    ? saleRecordForm.sellTotal\n                                    : 0\n                            }}&nbsp;&nbsp; 消费：{{\n                                saleRecordForm.sellTotalmoney\n                                    ? parseFloat(\n                                          saleRecordForm.sellTotalmoney\n                                      ).toFixed(2)\n                                    : 0\n                            }}元&nbsp;&nbsp;\n                            <span v-if=\"saleRecordForm.sellway\"\n                                >支付方式：{{\n                                    saleRecordForm.sellway == \"0\"\n                                        ? \"支付宝\"\n                                        : saleRecordForm.sellway == \"1\"\n                                        ? \"微信\"\n                                        : saleRecordForm.sellway == \"2\"\n                                        ? \"现金\"\n                                        : \"银行卡\"\n                                }}&nbsp;&nbsp;</span\n                            ><br>\n                            <span v-if=\"saleRecordForm.type == '1'\"\n                                >会员享受9折&nbsp;&nbsp;</span\n                            >\n                            <span v-if=\"saleRecordForm.sellTime\"\n                                >消费时间：{{\n                                    saleRecordForm.sellTime\n                                }}&nbsp;&nbsp; 操作者编号：{{\n                                    saleRecordForm.eid\n                                }}</span\n                            >\n                        </div>\n                    </el-col>\n                </el-row>\n                <el-row>\n                    <el-col :span=\"24\">\n                        <el-form-item>\n                            <el-button\n                                type=\"info\"\n                                @click=\"detailSaleRecordsVisable = false\"\n                                >关闭</el-button\n                            >\n                        </el-form-item>\n                    </el-col>\n                </el-row>\n            </el-form>\n        </el-dialog>\n    </div>\n</template>\n<script>\nimport { popup } from \"@/assets/js/common\";\nimport {\n    delSaleRecords,\n    queryPageByQoSaleRecords,\n} from \"@/api/sale_management/saleRecordsApi\";\n\nexport default {\n    data() {\n        return {\n            detailSaleRecordsVisable: false,\n            searchForm: {},\n            tableData: [],\n            saleRecordForm: {\n                cn: \"\",\n                sellway: \"\",\n                info: \"\",\n                sellTotal: \"\",\n                sellTotalmoney: \"\",\n                type: \"0\",\n                detailSaleRecords: [],\n            },\n        };\n    },\n    methods: {\n        init() {\n            if (this.searchForm.startSellTime > this.searchForm.endSellTime) {\n                popup(\"结束时间不能小于开始时间\", \"warning\");\n                return;\n            }\n            queryPageByQoSaleRecords(this.searchForm).then((res) => {\n                res = res.data;\n                if (res.code == 200) {\n                    this.tableData = res.data.records;\n                    this.searchForm.total = res.data.total;\n                    this.searchForm.pageSize = res.data.size;\n                    this.searchForm.currentPage = res.data.current;\n                } else {\n                    popup(res.msg, \"error\");\n                }\n            });\n        },\n        del(cn) {\n            this.$confirm(\"确定删除这条记录？\", \"警示\", {\n                confirmButtonText: \"确定\",\n                cancelButtonText: \"取消\",\n                type: \"warning\",\n            })\n                .then(() => {\n                    delSaleRecords({ cn: cn }).then((res) => {\n                        res = res.data;\n                        if (res.code == 200) {\n                            popup(\"操作成功\");\n                            this.init();\n                        } else {\n                            popup(res.msg, \"error\");\n                        }\n                    });\n                })\n                .catch(() => {\n                    this.$message({\n                        type: \"info\",\n                        message: \"已取消操作\",\n                    });\n                });\n        },\n        detailBtn(row) {\n            this.saleRecordForm = {\n                cn: \"\",\n                sellway: \"\",\n                info: \"\",\n                sellTotal: \"\",\n                sellTotalmoney: \"\",\n                type: \"0\",\n                detailSaleRecords: [],\n            };\n            this.saleRecordForm = { ...row };\n            this.detailSaleRecordsVisable = true;\n        },\n        /*分页*/\n        handleSizeChange(val) {\n            this.searchForm.pageSize = val;\n            this.init();\n            console.log(`每页 ${val} 条`);\n        },\n        handleCurrentChange(val) {\n            this.searchForm.currentPage = val;\n            this.init();\n            console.log(`当前页: ${val}`);\n        },\n        submitSearchForm() {\n            this.init();\n        },\n    },\n    mounted() {\n        this.init();\n    },\n};\n</script>\n<style>\n.searchForm {\n    height: 134px;\n}\n\n.table {\n    height: 320px;\n}\n\n.pageUtils {\n    position: absolute;\n    width: 84%;\n    margin: 2px auto;\n    height: 43px;\n}\n</style>"],"mappings":"AAiXA,SAAAA,KAAA;AACA,SACAC,cAAA,EACAC,wBAAA,QACA;AAEA;EACAC,KAAA;IACA;MACAC,wBAAA;MACAC,UAAA;MACAC,SAAA;MACAC,cAAA;QACAC,EAAA;QACAC,OAAA;QACAC,IAAA;QACAC,SAAA;QACAC,cAAA;QACAC,IAAA;QACAC,iBAAA;MACA;IACA;EACA;EACAC,OAAA;IACAC,KAAA;MACA,SAAAX,UAAA,CAAAY,aAAA,QAAAZ,UAAA,CAAAa,WAAA;QACAlB,KAAA;QACA;MACA;MACAE,wBAAA,MAAAG,UAAA,EAAAc,IAAA,CAAAC,GAAA;QACAA,GAAA,GAAAA,GAAA,CAAAjB,IAAA;QACA,IAAAiB,GAAA,CAAAC,IAAA;UACA,KAAAf,SAAA,GAAAc,GAAA,CAAAjB,IAAA,CAAAmB,OAAA;UACA,KAAAjB,UAAA,CAAAkB,KAAA,GAAAH,GAAA,CAAAjB,IAAA,CAAAoB,KAAA;UACA,KAAAlB,UAAA,CAAAmB,QAAA,GAAAJ,GAAA,CAAAjB,IAAA,CAAAsB,IAAA;UACA,KAAApB,UAAA,CAAAqB,WAAA,GAAAN,GAAA,CAAAjB,IAAA,CAAAwB,OAAA;QACA;UACA3B,KAAA,CAAAoB,GAAA,CAAAQ,GAAA;QACA;MACA;IACA;IACAC,IAAArB,EAAA;MACA,KAAAsB,QAAA;QACAC,iBAAA;QACAC,gBAAA;QACAnB,IAAA;MACA,GACAM,IAAA;QACAlB,cAAA;UAAAO,EAAA,EAAAA;QAAA,GAAAW,IAAA,CAAAC,GAAA;UACAA,GAAA,GAAAA,GAAA,CAAAjB,IAAA;UACA,IAAAiB,GAAA,CAAAC,IAAA;YACArB,KAAA;YACA,KAAAgB,IAAA;UACA;YACAhB,KAAA,CAAAoB,GAAA,CAAAQ,GAAA;UACA;QACA;MACA,GACAK,KAAA;QACA,KAAAC,QAAA;UACArB,IAAA;UACAsB,OAAA;QACA;MACA;IACA;IACAC,UAAAC,GAAA;MACA,KAAA9B,cAAA;QACAC,EAAA;QACAC,OAAA;QACAC,IAAA;QACAC,SAAA;QACAC,cAAA;QACAC,IAAA;QACAC,iBAAA;MACA;MACA,KAAAP,cAAA;QAAA,GAAA8B;MAAA;MACA,KAAAjC,wBAAA;IACA;IACA;IACAkC,iBAAAC,GAAA;MACA,KAAAlC,UAAA,CAAAmB,QAAA,GAAAe,GAAA;MACA,KAAAvB,IAAA;MACAwB,OAAA,CAAAC,GAAA,OAAAF,GAAA;IACA;IACAG,oBAAAH,GAAA;M