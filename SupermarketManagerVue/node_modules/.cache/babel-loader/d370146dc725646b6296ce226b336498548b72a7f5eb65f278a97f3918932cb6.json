{"ast":null,"code":"import { popup } from \"@/assets/js/common\";\nimport { queryGoodsStoreById, queryPageByQo, updateInventory } from \"@/api/goods_management/goods_store/goodsStoreApi\";\nexport default {\n  data() {\n    return {\n      editVisible: false,\n      /*表格*/\n      tableData: [],\n      editForm: {},\n      searchForm: {\n        total: 0,\n        currentPage: 1,\n        pageSize: 3,\n        state: '0'\n      },\n      rules: {\n        inventory: [{\n          required: true,\n          message: '库存量不能为空',\n          trigger: 'blur'\n        }],\n        shelves: [{\n          required: true,\n          message: '货架数量不能为空',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  methods: {\n    init() {\n      queryPageByQo(this.searchForm).then(res => {\n        res = res.data;\n        if (res.code == 200) {\n          console.log(res.data);\n          this.tableData = res.data.records;\n          this.searchForm.total = res.data.total;\n          this.searchForm.pageSize = res.data.size;\n          this.searchForm.currentPage = res.data.current;\n        } else {\n          popup(res.msg, \"error\");\n        }\n      });\n    },\n    /*分页*/\n    handleSizeChange(val) {\n      this.searchForm.pageSize = val;\n      this.init();\n      console.log(`每页 ${val} 条`);\n    },\n    handleCurrentChange(val) {\n      this.searchForm.currentPage = val;\n      this.init();\n      console.log(`当前页: ${val}`);\n    },\n    submitSearchForm() {\n      this.init();\n    },\n    editBtn(id) {\n      queryGoodsStoreById({\n        id: id\n      }).then(res => {\n        res = res.data;\n        if (res.code == 200) {\n          this.editForm = {\n            ...res.data\n          };\n          this.editVisible = true;\n        } else {\n          popup(res.msg, \"error\");\n        }\n      });\n    },\n    submitEditForm(formName) {\n      this.$refs[formName].validate(valid => {\n        if (valid) {\n          updateInventory(this.editForm).then(res => {\n            res = res.data;\n            if (res.code == 200) {\n              popup(\"更新成功\");\n              this.editForm = {};\n              this.editVisible = false;\n              this.init();\n            } else {\n              popup(res.msg, \"error\");\n            }\n          });\n        }\n      });\n    },\n    closeEdit(formName) {\n      this.$refs[formName].resetFields();\n      this.editForm = {};\n      this.editVisible = false;\n    }\n  },\n  mounted() {\n    this.init();\n  }\n};","map":{"version":3,"names":["popup","queryGoodsStoreById","queryPageByQo","updateInventory","data","editVisible","tableData","editForm","searchForm","total","currentPage","pageSize","state","rules","inventory","required","message","trigger","shelves","methods","init","then","res","code","console","log","records","size","current","msg","handleSizeChange","val","handleCurrentChange","submitSearchForm","editBtn","id","submitEditForm","formName","$refs","validate","valid","closeEdit","resetFields","mounted"],"sources":["src/views/goods_management/goods_store/List.vue"],"sourcesContent":["<template>\n    <div>\n        <div class=\"searchForm\">\n            <div class=\"column\">\n                <span>商品名：</span>\n                <el-input style=\"height: 21px;width: 300px\" placeholder=\"请输入商品名\" v-model=\"searchForm.name\"/>\n            </div>\n            <div class=\"column\">\n                <el-button type=\"success\" @click=\"submitSearchForm\">搜索</el-button>\n            </div>\n        </div>\n        <div class=\"table\">\n            <el-table\n                    :data=\"tableData\"\n                    style=\"width: 100%;\"\n                    \n                    size=\"medium\">\n                <el-table-column\n                        width=\"200\"\n                        prop=\"id\"\n                        label=\"编号\">\n                </el-table-column>\n                <el-table-column\n                        prop=\"coverUrl\"\n                        label=\"封面\">\n                    <template v-slot=\"scope\">\n                        <img height=\"60px\" :src=\"scope.row.coverUrl\">\n                    </template>\n                </el-table-column>\n                <el-table-column\n                        prop=\"name\"\n                        label=\"商品名\">\n                </el-table-column>\n                <el-table-column\n                        prop=\"inventory\"\n                        label=\"基本库存量\">\n                </el-table-column>\n                <el-table-column\n                        prop=\"shelves\"\n                        label=\"货架基本数量\">\n                </el-table-column>\n                <el-table-column\n                        label=\"操作\">\n                    <template v-slot=\"scope\">\n                        <el-button size=\"mini\" type=\"success\" @click=\"editBtn(scope.row.id)\" plain>修改库存量</el-button>\n                    </template>\n                </el-table-column>\n            </el-table>\n            <div class=\"pageUtils\">\n                <el-pagination\n                        @size-change=\"handleSizeChange\"\n                        @current-change=\"handleCurrentChange\"\n                        :current-page.sync=\"searchForm.currentPage\"\n                        :page-sizes=\"[3, 5, 7, 9]\"\n                        :page-size=\"searchForm.pageSize\"\n                        layout=\"total,sizes, prev, pager, next,jumper\"\n                        :total=\"searchForm.total\">\n                </el-pagination>\n            </div>\n        </div>\n        <!--修改库存量-->\n        <el-dialog\n                title=\"修改商品库存量\"\n                :visible.sync=\"editVisible\"\n                width=\"60%\">\n            <el-form :model=\"editForm\" :rules=\"rules\" ref=\"editForm\" label-width=\"150px\" class=\"demo-ruleForm\">\n                <el-form-item style=\"width: 60%\" label=\"商品名：\">\n                    <el-input disabled v-model=\"editForm.name\" placeholder=\"如：农夫山泉\"></el-input>\n                </el-form-item>\n\n                <el-form-item label=\"基本库存量：\" style=\"width: 60%\" prop=\"inventory\">\n                    <el-input type=\"number\" @change=\"()=>{\n                                if(editForm.inventory<=0){\n                                    editForm.inventory=1\n                                }\n                            }\" v-model=\"editForm.inventory\" placeholder=\"如：10\"></el-input>\n                </el-form-item>\n                <el-form-item label=\"货架基本数量：\" style=\"width: 60%\" prop=\"shelves\">\n                    <el-input type=\"number\" @change=\"()=>{\n                                if(editForm.shelves<=0){\n                                    editForm.shelves=1\n                                }\n                            }\" v-model=\"editForm.shelves\" placeholder=\"如：10\"></el-input>\n                </el-form-item>\n                <el-form-item style=\"width: 60%\" >\n                    <el-button type=\"primary\" @click=\"submitEditForm('editForm')\">提交</el-button>\n                    <el-button @click=\"closeEdit('editForm')\">关闭</el-button>\n                </el-form-item>\n\n            </el-form>\n        </el-dialog>\n    </div>\n</template>\n<script>\n    import {popup} from \"@/assets/js/common\";\n    import {\n        queryGoodsStoreById,\n        queryPageByQo,\n        updateInventory\n    } from \"@/api/goods_management/goods_store/goodsStoreApi\";\n\n    export default {\n        data() {\n            return {\n                editVisible: false,\n                /*表格*/\n                tableData: [],\n                editForm: {},\n                searchForm: {\n                    total: 0,\n                    currentPage: 1,\n                    pageSize: 3,\n                    state: '0'\n                },\n                rules: {\n                    inventory: [\n                        {required: true, message: '库存量不能为空', trigger: 'blur'},\n                    ],\n                    shelves:[\n                        {required: true, message: '货架数量不能为空', trigger: 'blur'},\n                    ]\n                }\n            }\n        },\n        methods: {\n\n            init() {\n                queryPageByQo(this.searchForm).then(res => {\n                    res = res.data\n                    if (res.code == 200) {\n                        console.log(res.data)\n                        this.tableData = res.data.records\n                        this.searchForm.total = res.data.total\n                        this.searchForm.pageSize = res.data.size\n                        this.searchForm.currentPage = res.data.current\n                    } else {\n                        popup(res.msg, \"error\")\n                    }\n                })\n            },\n            /*分页*/\n            handleSizeChange(val) {\n                this.searchForm.pageSize = val\n                this.init()\n                console.log(`每页 ${val} 条`);\n            },\n            handleCurrentChange(val) {\n                this.searchForm.currentPage = val\n                this.init()\n                console.log(`当前页: ${val}`);\n            },\n            submitSearchForm() {\n                this.init()\n            },\n            editBtn(id) {\n                queryGoodsStoreById({id: id}).then(res => {\n                    res = res.data\n                    if (res.code == 200) {\n                        this.editForm = {...res.data}\n                        this.editVisible = true\n                    } else {\n                        popup(res.msg, \"error\")\n                    }\n                })\n\n            },\n            submitEditForm(formName) {\n                this.$refs[formName].validate((valid) => {\n                    if (valid) {\n                        updateInventory(this.editForm).then(res => {\n                            res = res.data\n                            if (res.code == 200) {\n                                popup(\"更新成功\")\n                                this.editForm = {}\n                                this.editVisible=false\n                                this.init()\n                            } else {\n                                popup(res.msg, \"error\")\n                            }\n                        })\n                    }\n                })\n            },\n            closeEdit(formName) {\n                this.$refs[formName].resetFields()\n                this.editForm = {}\n                this.editVisible = false\n            }\n        },\n        mounted() {\n            this.init()\n        }\n    }\n</script>\n<style>\n    .searchForm {\n        height: 134px;\n    }\n\n    .table {\n        height: 320px;\n    }\n\n    .pageUtils {\n        position: absolute;\n        width: 84%;\n        margin: 2px auto;\n        height: 43px;\n    }\n\n    .searchForm {\n        display: inline-block;\n        margin: 0 auto;\n        height: 51px;\n        line-height: 51px;\n    }\n\n    .searchForm .column {\n        display: inline-block;\n        margin: 0 auto;\n        margin-right: 2px;\n        line-height: 48px;\n        height: 51px;\n    }\n</style>"],"mappings":"AA8FA,SAAAA,KAAA;AACA,SACAC,mBAAA,EACAC,aAAA,EACAC,eAAA,QACA;AAEA;EACAC,KAAA;IACA;MACAC,WAAA;MACA;MACAC,SAAA;MACAC,QAAA;MACAC,UAAA;QACAC,KAAA;QACAC,WAAA;QACAC,QAAA;QACAC,KAAA;MACA;MACAC,KAAA;QACAC,SAAA,GACA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAC,OAAA,GACA;UAAAH,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAE,OAAA;IAEAC,KAAA;MACAlB,aAAA,MAAAM,UAAA,EAAAa,IAAA,CAAAC,GAAA;QACAA,GAAA,GAAAA,GAAA,CAAAlB,IAAA;QACA,IAAAkB,GAAA,CAAAC,IAAA;UACAC,OAAA,CAAAC,GAAA,CAAAH,GAAA,CAAAlB,IAAA;UACA,KAAAE,SAAA,GAAAgB,GAAA,CAAAlB,IAAA,CAAAsB,OAAA;UACA,KAAAlB,UAAA,CAAAC,KAAA,GAAAa,GAAA,CAAAlB,IAAA,CAAAK,KAAA;UACA,KAAAD,UAAA,CAAAG,QAAA,GAAAW,GAAA,CAAAlB,IAAA,CAAAuB,IAAA;UACA,KAAAnB,UAAA,CAAAE,WAAA,GAAAY,GAAA,CAAAlB,IAAA,