e-column\n                        prop=\"tel\"\n                        :show-overflow-tooltip=\"true\"\n                        label=\"联系电话\">\n                </el-table-column>\n                <el-table-column\n                        prop=\"info\"\n                        :show-overflow-tooltip=\"true\"\n                        label=\"备注\">\n                </el-table-column>\n                <el-table-column\n                        width=\"200px\"\n                        label=\"操作\">\n                    <template v-slot=\"scope\">\n                        <el-button type=\"warning\" @click=\"editBtn(scope.row.cn)\" plain>修改</el-button>\n                        <el-button type=\"danger\" @click=\"delBtn(scope.row.cn)\" plain>删除</el-button>\n                    </template>\n                </el-table-column>\n            </el-table>\n            <div class=\"pageUtils\">\n                <el-pagination\n                        @size-change=\"handleSizeChange\"\n                        @current-change=\"handleCurrentChange\"\n                        :current-page.sync=\"searchForm.currentPage\"\n                        :page-sizes=\"[3, 5, 7, 9]\"\n                        :page-size=\"searchForm.pageSize\"\n                        layout=\"total,sizes, prev, pager, next,jumper\"\n                        :total=\"searchForm.total\">\n                </el-pagination>\n            </div>\n        </div>\n        <!--创建弹出框-->\n        <el-dialog\n                title=\"创建供货商信息\"\n                :visible.sync=\"newVisable\"\n                width=\"50%\">\n            <el-form :model=\"newForm\" :rules=\"rules\" ref=\"newForm\" label-width=\"100px\" class=\"demo-ruleForm\">\n                <el-form-item label=\"名称：\" prop=\"name\">\n                    <el-input v-model=\"newForm.name\" placeholder=\"供应商名称\"></el-input>\n                </el-form-item>\n                <el-form-item label=\"地址：\" prop=\"address\">\n                    <el-input type=\"text\" 