{"ast":null,"code":"import { popup } from \"@/assets/js/common\";\nimport { delExchangePointProducts, queryOptionsMemberPhone, queryPageByQoExchangePointProducts } from \"@/api/sale_management/exchange_point_products/exchangePointProductsApi\";\nexport default {\n  data() {\n    return {\n      searchForm: {},\n      tableData: [],\n      options_memberPhone: []\n    };\n  },\n  methods: {\n    queryOptionsMemberPhone() {\n      queryOptionsMemberPhone().then(res => {\n        res = res.data;\n        if (res.code == 200) {\n          this.options_memberPhone = res.data;\n        } else {\n          popup(res.msg, \"error\");\n        }\n      });\n    },\n    init() {\n      if (this.searchForm.startTime > this.searchForm.endTime) {\n        popup(\"结束时间不能小于开始时间\", \"warning\");\n        return;\n      }\n      queryPageByQoExchangePointProducts(this.searchForm).then(res => {\n        res = res.data;\n        if (res.code == 200) {\n          this.tableData = res.data.records;\n          this.searchForm.total = res.data.total;\n          this.searchForm.pageSize = res.data.size;\n          this.searchForm.currentPage = res.data.current;\n        } else {\n          popup(res.msg, \"error\");\n        }\n      });\n    },\n    del(cn) {\n      this.$confirm(\"确定删除这条记录？\", \"警示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(() => {\n        delExchangePointProducts({\n          cn: cn\n        }).then(res => {\n          res = res.data;\n          if (res.code == 200) {\n            popup(\"操作成功\");\n            this.init();\n          } else {\n            popup(res.msg, \"error\");\n          }\n        });\n      }).catch(() => {\n        this.$message({\n          type: \"info\",\n          message: \"已取消操作\"\n        });\n      });\n    },\n    /*分页*/\n    handleSizeChange(val) {\n      this.searchForm.pageSize = val;\n      this.init();\n      console.log(`每页 ${val} 条`);\n    },\n    handleCurrentChange(val) {\n      this.searchForm.currentPage = val;\n      this.init();\n      console.log(`当前页: ${val}`);\n    },\n    submitSearchForm() {\n      this.init();\n    }\n  },\n  mounted() {\n    this.init();\n    this.queryOptionsMemberPhone();\n  }\n};","map":{"version":3,"names":["popup","delExchangePointProducts","queryOptionsMemberPhone","queryPageByQoExchangePointProducts","data","searchForm","tableData","options_memberPhone","methods","then","res","code","msg","init","startTime","endTime","records","total","pageSize","size","currentPage","current","del","cn","$confirm","confirmButtonText","cancelButtonText","type","catch","$message","message","handleSizeChange","val","console","log","handleCurrentChange","submitSearchForm","mounted"],"sources":["src/views/sale_management/exchange_point_products_records/List.vue"],"sourcesContent":["<template>\n    <div id=\"sale_records\">\n        <el-breadcrumb separator=\"/\">\n            <el-breadcrumb-item :to=\"{ path: '/index' }\"\n                >首页</el-breadcrumb-item\n            >\n            <el-breadcrumb-item :to=\"{ path: '/sale_management/sale_cmd/list' }\"\n                >销售管理\n            </el-breadcrumb-item>\n            <el-breadcrumb-item\n                >积分兑换记录</el-breadcrumb-item\n            > </el-breadcrumb\n        ><br />\n        <el-row>\n            <el-col :span=\"4\">\n                <el-input\n                    placeholder=\"兑换订单编号\"\n                    v-model=\"searchForm.cn\"\n                />\n            </el-col>\n            <el-col :span=\"7\">\n                <el-select\n                    v-model=\"searchForm.memberId\"\n                    placeholder=\"请选择会员账号\"\n                    filterable\n                    @change=\"$forceUpdate()\"\n                    clearable\n                >\n                    <el-option\n                        v-for=\"item in options_memberPhone\"\n                        :key=\"item.id\"\n                        :label=\"item.name\"\n                        :value=\"item.id\"\n                    >\n                    </el-option>\n                </el-select>\n            </el-col>\n            <el-col :span=\"10\">\n                <span>兑换时间：</span>\n                <el-date-picker\n                    style=\"width: 140px\"\n                    v-model=\"searchForm.startTime\"\n                    type=\"date\"\n                    value-format=\"yyyy-MM-dd\"\n                    placeholder=\"起始时间\"\n                >\n                </el-date-picker>\n                -\n                <el-date-picker\n                    v-model=\"searchForm.endTime\"\n                    style=\"width: 140px\"\n                    value-format=\"yyyy-MM-dd\"\n                    type=\"date\"\n                    placeholder=\"结束时间\"\n                >\n                </el-date-picker>\n            </el-col>\n        </el-row>\n        <br />\n        <el-row>\n            <el-col :span=\"24\" style=\"text-align: left\">\n                <el-button\n                    type=\"success\"\n                    @click=\"submitSearchForm\"\n                    style=\"font-size: 18px\"\n                >\n                    <i class=\"iconfont icon-r-find\" style=\"font-size: 18px\">\n                    </i>\n                    搜索</el-button\n                >\n            </el-col>\n        </el-row>\n        <br />\n        <div class=\"table\">\n            <el-table :data=\"tableData\" style=\"width: 100%\" size=\"medium\">\n                <el-table-column prop=\"cn\" width=\"200\" label=\"编号\">\n                </el-table-column>\n                <el-table-column prop=\"goodsCoverUrl\" label=\"商品封面\">\n                    <template v-slot=\"scope\">\n                        <img\n                            :src=\"BaseApi + scope.row.goodsCoverUrl\"\n                            alt=\"兑换商品图片\"\n                            height=\"50px\"\n                        />\n                    </template>\n                </el-table-column>\n                <el-table-column prop=\"goodsName\" label=\"商品\">\n                </el-table-column>\n                <el-table-column prop=\"integral\" label=\"积分\">\n                </el-table-column>\n                <el-table-column\n                    width=\"200\"\n                    prop=\"memberPhone\"\n                    label=\"会员账号\"\n                >\n                </el-table-column>\n                <el-table-column prop=\"updateTime\" width=\"200\" label=\"兑换时间\">\n                </el-table-column>\n                <el-table-column prop=\"updateby\" label=\"操作者\">\n                </el-table-column>\n                <el-table-column width=\"400\" label=\"操作\">\n                    <template v-slot=\"scope\">\n                        <el-button\n                            type=\"danger\"\n                            @click=\"del(scope.row.cn)\"\n                            plain\n                            >删除\n                        </el-button>\n                    </template>\n                </el-table-column>\n            </el-table>\n            <div class=\"pageUtils\">\n                <el-pagination\n                    @size-change=\"handleSizeChange\"\n                    @current-change=\"handleCurrentChange\"\n                    :current-page.sync=\"searchForm.currentPage\"\n                    :page-sizes=\"[3, 5, 7, 9]\"\n                    :page-size=\"searchForm.pageSize\"\n                    layout=\"total,sizes, prev, pager, next,jumper\"\n                    :total=\"searchForm.total\"\n                >\n                </el-pagination>\n            </div>\n        </div>\n    </div>\n</template>\n<script>\nimport { popup } from \"@/assets/js/common\";\nimport {\n    delExchangePointProducts,\n    queryOptionsMemberPhone,\n    queryPageByQoExchangePointProducts,\n} from \"@/api/sale_management/exchange_point_products/exchangePointProductsApi\";\n\nexport default {\n    data() {\n        return {\n            searchForm: {},\n            tableData: [],\n            options_memberPhone: [],\n        };\n    },\n    methods: {\n        queryOptionsMemberPhone() {\n            queryOptionsMemberPhone().then((res) => {\n                res = res.data;\n                if (res.code == 200) {\n                    this.options_memberPhone = res.data;\n                } else {\n                    popup(res.msg, \"error\");\n                }\n            });\n        },\n        init() {\n            if (this.searchForm.startTime > this.searchForm.endTime) {\n                popup(\"结束时间不能小于开始时间\", \"warning\");\n                return;\n            }\n            queryPageByQoExchangePointProducts(this.searchForm).then((res) => {\n                res = res.data;\n                if (res.code == 200) {\n                    this.tableData = res.data.records;\n                    this.searchForm.total = res.data.total;\n                    this.searchForm.pageSize = res.data.size;\n                    this.searchForm.currentPage = res.data.current;\n                } else {\n                    popup(res.msg, \"error\");\n                }\n            });\n        },\n        del(cn) {\n            this.$confirm(\"确定删除这条记录？\", \"警示\", {\n                confirmButtonText: \"确定\",\n                cancelButtonText: \"取消\",\n                type: \"warning\",\n            })\n                .then(() => {\n                    delExchangePointProducts({ cn: cn }).then((res) => {\n                        res = res.data;\n                        if (res.code == 200) {\n                            popup(\"操作成功\");\n                            this.init();\n                        } else {\n                            popup(res.msg, \"error\");\n                        }\n                    });\n                })\n                .catch(() => {\n                    this.$message({\n                        type: \"info\",\n                        message: \"已取消操作\",\n                    });\n                });\n        },\n        /*分页*/\n        handleSizeChange(val) {\n            this.searchForm.pageSize = val;\n            this.init();\n            console.log(`每页 ${val} 条`);\n        },\n        handleCurrentChange(val) {\n            this.searchForm.currentPage = val;\n            this.init();\n            console.log(`当前页: ${val}`);\n        },\n        submitSearchForm() {\n            this.init();\n        },\n    },\n    mounted() {\n        this.init();\n        this.queryOptionsMemberPhone();\n    },\n};\n</script>\n<style>\n.searchForm {\n    height: 134px;\n}\n\n.table {\n    height: 320px;\n}\n\n.pageUtils {\n    position: absolute;\n    width: 84%;\n    margin: 2px auto;\n    height: 43px;\n}\n\n.searchForm {\n    display: inline-block;\n    margin: 0 auto;\n    height: 51px;\n    line-height: 51px;\n}\n\n.searchForm .column {\n    display: inline-block;\n    margin: 0 auto;\n    margin-right: 2px;\n    line-height: 48px;\n    height: 51px;\n}\n</style>"],"mappings":"AA+HA,SAAAA,KAAA;AACA,SACAC,wBAAA,EACAC,uBAAA,EACAC,kCAAA,QACA;AAEA;EACAC,KAAA;IACA;MACAC,UAAA;MACAC,SAAA;MACAC,mBAAA;IACA;EACA;EACAC,OAAA;IACAN,wBAAA;MACAA,uBAAA,GAAAO,IAAA,CAAAC,GAAA;QACAA,GAAA,GAAAA,GAAA,CAAAN,IAAA;QACA,IAAAM,GAAA,CAAAC,IAAA;UACA,KAAAJ,mBAAA,GAAAG,GAAA,CAAAN,IAAA;QACA;UACAJ,KAAA,CAAAU,GAAA,CAAAE,GAAA;QACA;MACA;IACA;IACAC,KAAA;MACA,SAAAR,UAAA,CAAAS,SAAA,QAAAT,UAAA,CAAAU,OAAA;QACAf,KAAA;QACA;MACA;MACAG,kCAAA,MAAAE,UAAA,EAAAI,IAAA,CAAAC,GAAA;QACAA,GAAA,GAAAA,GAAA,CAAAN,IAAA;QACA,IAAAM,GAAA,CAAAC,IAAA;UACA,KAAAL,SAAA,GAAAI,GAAA,CAAAN,IAAA,CAAAY,OAAA;UACA,KAAAX,UAAA,CAAAY,KAAA,GAAAP,GAAA,CAAAN,IAAA,CAAAa,KAAA;UACA,KAAAZ,UAAA,CAAAa,QAAA,GAAAR,GAAA,CAAAN,IAAA,CAAAe,IAAA;UACA,KAAAd,UAAA,CAAAe,WAAA,GAAAV,GAAA,CAAAN,IAAA,CAAAiB,OAAA;QACA;UACArB,KAAA,CAAAU,GAAA,CAAAE,GAAA;QACA;MACA;IACA;IACAU,IAAAC,EAAA;MACA,KAAAC,QAAA;QACAC,iBAAA;QACAC,gBAAA;QACAC,IAAA;MACA,GACAlB,IAAA;QACAR,wBAAA;UAAAsB,EAAA,EAAAA;QAAA,GAAAd,IAAA,CAA